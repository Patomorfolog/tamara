[32m+[m[32mfunction sibling( cur, dir ) {[m
[32m+[m	[32mwhile ( (cur = cur[dir]) && cur.nodeType !== 1 ) {}[m
[32m+[m	[32mreturn cur;[m
[32m+[m[32m}[m
