[32m+[m
[32m+[m[32m// Print utilities[m
[32m+[m[32m//[m
[32m+[m[32m// Media queries are placed on the inside to be mixin-friendly.[m
[32m+[m
[32m+[m[32m// Note: Deprecated .visible-print as of v3.2.0[m
[32m+[m[32m.visible-print {[m
[32m+[m[32m  .responsive-invisibility();[m
[32m+[m
[32m+[m[32m  @media print {[m
[32m+[m[32m    .responsive-visibility();[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.visible-print-block {[m
[32m+[m[32m  display: none !important;[m
[32m+[m
[32m+[m[32m  @media print {[m
[32m+[m[32m    display: block !important;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.visible-print-inline {[m
[32m+[m[32m  display: none !important;[m
[32m+[m
[32m+[m[32m  @media print {[m
[32m+[m[32m    display: inline !important;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.visible-print-inline-block {[m
[32m+[m[32m  display: none !important;[m
[32m+[m
