[1mdiff --git a/bower_components/jquery/src/attributes/attr.js b/bower_components/jquery/src/attributes/attr.js[m
[1mnew file mode 100644[m
[1mindex 0000000..a4414d1[m
[1m--- /dev/null[m
[1m+++ b/bower_components/jquery/src/attributes/attr.js[m
[36m@@ -0,0 +1,141 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"../core",[m
[32m+[m	[32m"../var/rnotwhite",[m
[32m+[m	[32m"../var/strundefined",[m
[32m+[m	[32m"../core/access",[m
[32m+[m	[32m"./support",[m
[32m+[m	[32m"../selector"[m
[32m+[m[32m], function( jQuery, rnotwhite, strundefined, access, support ) {[m
[32m+[m
[32m+[m[32mvar nodeHook, boolHook,[m
[32m+[m	[32mattrHandle = jQuery.expr.attrHandle;[m
[32m+[m
[32m+[m[32mjQuery.fn.extend({[m
[32m+[m	[32mattr: function( name, value ) {[m
[32m+[m		[32mreturn access( this, jQuery.attr, name, value, arguments.length > 1 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveAttr: function( name ) {[m
[32m+[m		[32mreturn this.each(function() {[m
[32m+[m			[32mjQuery.removeAttr( this, name );[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mjQuery.extend({[m
[32m+[m	[32mattr: function( elem, name, value ) {[m
[32m+[m		[32mvar hooks, ret,[m
[32m+[m			[32mnType = elem.nodeType;[m
[32m+[m
[32m+[m		[32m// don't get/set attributes on text, comment and attribute nodes[m
[32m+[m		[32mif ( !elem || nType === 3 || nType === 8 || nType === 2 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Fallback to prop when attributes are not supported[m
[32m+[m		[32mif ( typeof elem.getAttribute === strundefined ) {[m
[32m+[m			[32mreturn jQuery.prop( elem, name, value );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// All attributes are lowercase[m
[32m+[m		[32m// Grab necessary hook if one is defined[m
[32m+[m		[32mif ( nType !== 1 || !jQuery.isXMLDoc( elem ) ) {[m
[32m+[m			[32mname = name.toLowerCase();[m
[32m+[m			[32mhooks = jQuery.attrHooks[ name ] ||[m
[32m+[m				[32m( jQuery.expr.match.bool.test( name ) ? boolHook : nodeHook );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( value !== undefined ) {[m
[32m+[m
[32m+[m			[32mif ( value === null ) {[m
[32m+[m				[32mjQuery.removeAttr( elem, name );[m
[32m+[m
[32m+[m			[32m} else if ( hooks && "set" in hooks && (ret = hooks.set( elem, value, name )) !== undefined ) {[m
[32m+[m				[32mreturn ret;[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32melem.setAttribute( name, value + "" );[m
[32m+[m				[32mreturn value;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m} else if ( hooks && "get" in hooks && (ret = hooks.get( elem, name )) !== null ) {[m
