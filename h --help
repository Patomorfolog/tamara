[1mdiff --git a/bower_components/bootstrap/grunt/bs-commonjs-generator.js b/bower_components/bootstrap/grunt/bs-commonjs-generator.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b0642cd[m
[1m--- /dev/null[m
[1m+++ b/bower_components/bootstrap/grunt/bs-commonjs-generator.js[m
[36m@@ -0,0 +1,23 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32mvar fs = require('fs');[m
[32m+[m[32mvar path = require('path');[m
[32m+[m
[32m+[m[32mvar COMMONJS_BANNER = '// This file is autogenerated via the `commonjs` Grunt task. You can require() this file in a CommonJS environment.\n';[m
[32m+[m
[32m+[m[32mmodule.exports = function generateCommonJSModule(grunt, srcFiles, destFilepath) {[m
[32m+[m[32m  var destDir = path.dirname(destFilepath);[m
[32m+[m
[32m+[m[32m  function srcPathToDestRequire(srcFilepath) {[m
[32m+[m[32m    var requirePath = path.relative(destDir, srcFilepath).replace(/\\/g, '/');[m
[32m+[m[32m    return 'require(\'' + requirePath + '\')';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var moduleOutputJs = COMMONJS_BANNER + srcFiles.map(srcPathToDestRequire).join('\n');[m
[32m+[m[32m  try {[m
[32m+[m[32m    fs.writeFileSync(destFilepath, moduleOutputJs);[m
[32m+[m[32m  }[m
[32m+[m[32m  catch (err) {[m
[32m+[m[32m    grunt.fail.warn(err);[m
[32m+[m[32m  }[m
[32m+[m[32m  grunt.log.writeln('File ' + destFilepath.cyan + ' created.');[m
[32m+[m[32m};[m
[1mdiff --git a/bower_components/bootstrap/grunt/bs-glyphicons-data-generator.js b/bower_components/bootstrap/grunt/bs-glyphicons-data-generator.js[m
[1mnew file mode 100644[m
[1mindex 0000000..339fd0f[m
[1m--- /dev/null[m
[1m+++ b/bower_components/bootstrap/grunt/bs-glyphicons-data-generator.js[m
[36m@@ -0,0 +1,41 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * Bootstrap Grunt task for Glyphicons data generation[m
[32m+[m[32m * http://getbootstrap.com[m
[32m+[m[32m * Copyright 2014 Twitter, Inc.[m
[32m+[m[32m * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)[m
[32m+[m[32m */[m
[32m+[m[32m'use strict';[m
[32m+[m[32mvar fs = require('fs');[m
[32m+[m
[32m+[m[32mmodule.exports = function generateGlyphiconsData(grunt) {[m
[32m+[m[32m  // Pass encoding, utf8, so `readFileSync` will return a string instead of a[m
[32m+[m[32m  // buffer[m
[32m+[m[32m  var glyphiconsFile = fs.readFileSync('less/glyphicons.less', 'utf8');[m
[32m+[m[32m  var glyphiconsLines = glyphiconsFile.split('\n');[m
[32m+[m
[32m+[m[32m  // Use any line that starts with ".glyphicon-" and capture the class name[m
[32m+[m[32m  var iconClassName = /^\.(glyphicon-[a-zA-Z0-9-]+)/;[m
[32m+[m[32m  var glyphiconsData = '# This file is generated via Grunt task. **Do not edit directly.**\n' +[m
[32m+[m[32m                       '# See the \'build-glyphicons-data\' task in Gruntfile.js.\n\n';[m
[32m+[m[32m  var glyphiconsYml = 'docs/_data/glyphicons.yml';[m
[32m+[m[32m  for (var i = 0, len = glyphiconsLines.length; i < len; i++) {[m
[32m+[m[32m    var match = glyphiconsLines[i].match(iconClassName);[m
[32m+[m
[32m+[m[32m    if (match !== null) {[m
[32m+[m[32m      glyphiconsData += '- ' + match[1] + '\n';[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Create the `_data` directory if it doesn't already exist[m
[32m+[m[32m  if (!fs.existsSync('docs/_data')) {[m
[32m+[m[32m    fs.mkdirSync('docs/_data');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    fs.writeFileSync(glyphiconsYml, glyphiconsData);[m
[32m+[m[32m  }[m
[32m+[m[32m  catch (err) {[m
[32m+[m[32m    grunt.fail.warn(err);[m
[32m+[m[32m  }[m
[32m+[m[32m  grunt.log.writeln('File ' + glyphiconsYml.cyan + ' created.');[m
[32m+[m[32m};[m
[1mdiff --git a/bower_components/bootstrap/grunt/bs-lessdoc-parser.js b/bower_components/bootstrap/grunt/bs-lessdoc-parser.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d6be774[m
[1m--- /dev/null[m
[1m+++ b/bower_components/bootstrap/grunt/bs-lessdoc-parser.js[m
[36m@@ -0,0 +1,238 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * Bootstrap Grunt task for parsing Less docstrings[m
[32m+[m[32m * http://getbootstrap.com[m
[32m+[m[32m * Copyright 2014 Twitter, Inc.[m
[32m+[m[32m * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)[m
[32m+[m[32m */[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar Markdown = require('markdown-it');[m
[32m+[m
[32m+[m[32mfunction markdown2html(markdownString) {[m
[32m+[m[32m  var md = new Markdown();[m
[32m+[m
[32m+[m[32m  // the slice removes the <p>...</p> wrapper output by Markdown processor[m
[32m+[m[32m  return md.render(markdownString.trim()).slice(3, -5);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32mMini-language:[m
[32m+[m[32m  //== This is a normal heading, which starts a section. Sections group variables together.[m
[32m+[m[32m  //## Optional description for the heading[m
[32m+[m
[32m+[m[32m  //=== This is a subheading.[m
[32m+[m
[32m+[m[32m  //** Optional description for the following variable. You **can** use Markdown in descriptions to discuss `<html>` stuff.[m
[32m+[m[32m  @foo: #fff;[m
[32m+[m
[32m+[m[32m  //-- This is a heading for a section whose variables shouldn't be customizable[m
[32m+[m
[32m+[m[32m  All other lines are ignored completely.[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m
[32m+[m[32mvar CUSTOMIZABLE_HEADING = /^[/]{2}={2}(.*)$/;[m
[32m+[m[32mvar UNCUSTOMIZABLE_HEADING = /^[/]{2}-{2}(.*)$/;[m
[32m+[m[32mvar SUBSECTION_HEADING = /^[/]{2}={3}(.*)$/;[m
[32m+[m[32mvar SECTION_DOCSTRING = /^[/]{2}#{2}(.+)$/;[m
[32m+[m[32mvar VAR_ASSIGNMENT = /^(@[a-zA-Z0-9_-]+):[ ]*([^ ;][^;]*);[ ]*$/;[m
[32m+[m[32mvar VAR_DOCSTRING = /^[/]{2}[*]{2}(.+)$/;[m
[32m+[m
[32m+[m[32mfunction Section(heading, customizable) {[m
[32m+[m[32m  this.heading = heading.trim();[m
[32m+[m[32m  this.id = this.heading.replace(/\s+/g, '-').toLowerCase();[m
[32m+[m[32m  this.customizable = customizable;[m
[32m+[m[32m  this.docstring = null;[m
[32m+[m[32m  this.subsections = [];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSection.prototype.addSubSection = function (subsection) {[m
[32m+[m[32m  this.subsections.push(subsection);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction SubSection(heading) {[m
[32m+[m[32m  this.heading = heading.trim();[m
[32m+[m[32m  this.id = this.heading.replace(/\s+/g, '-').toLowerCase();[m
[32m+[m[32m  this.variables = [];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mSubSection.prototype.addVar = function (variable) {[m
[32m+[m[32m  this.variables.push(variable);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction VarDocstring(markdownString) {[m
[32m+[m[32m  this.html = markdown2html(markdownString);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction SectionDocstring(markdownString) {[m
[32m+[m[32m  this.html = markdown2html(markdownString);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction Variable(name, defaultValue) {[m
[32m+[m[32m  this.name = name;[m
[32m+[m[32m  this.defaultValue = defaultValue;[m
[32m+[m[32m  this.docstring = null;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction Tokenizer(fileContent) {[m
[32m+[m[32m  this._lines = fileContent.split('\n');[m
[32m+[m[32m  this._next = undefined;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mTokenizer.prototype.unshift = function (token) {[m
[32m+[m[32m  if (this._next !== undefined) {[m
[32m+[m[32m    throw new Error('Attempted to unshift twice!');[m
[32m+[m[32m  }[m
[32m+[m[32m  this._next = token;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mTokenizer.prototype._shift = function () {[m
[32m+[m[32m  // returning null signals EOF[m
[32m+[m[32m  // returning undefined means the line was ignored[m
[32m+[m[32m  if (this._next !== undefined) {[m
[32m+[m[32m    var result = this._next;[m
[32m+[m[32m    this._next = undefined;[m
[32m+[m[32m    return result;[m
[32m+[m[32m  }[m
[32m+[m[32m  if (this._lines.length <= 0) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m[32m  var line = this._lines.shift();[m
[32m+[m[32m  var match = null;[m
[32m+[m[32m  match = SUBSECTION_HEADING.exec(line);[m
[32m+[m[32m  if (match !== null) {[m
[32m+[m[32m    return new SubSection(match[1]);[m
[32m+[m[32m  }[m
[32m+[m[32m  match = CUSTOMIZABLE_HEADING.exec(line);[m
[32m+[m[32m  if (match !== null) {[m
[32m+[m[32m    return new Section(match[1], true);[m
[32m+[m[32m  }[m
[32m+[m[32m  match = UNCUSTOMIZABLE_HEADING.exec(line);[m
[32m+[m[32m  if (match !== null) {[m
[32m+[m[32m    return new Section(match[1], false);[m
[32m+[m[32m  }[m
[32m+[m[32m  match = SECTION_DOCSTRING.exec(line);[m
[32m+[m[32m  if (match !== null) {[m
[32m+[m[32m    return new SectionDocstring(match[1]);[m
[32m+[m[32m  }[m
[32m+[m[32m  match = VAR_DOCSTRING.exec(line);[m
[32m+[m[32m  if (match !== null) {[m
[32m+[m[32m    return new VarDocstring(match[1]);[m
[32m+[m[32m  }[m
[32m+[m[32m  var commentStart = line.lastIndexOf('//');[m
[32m+[m[32m  var varLine = (commentStart === -1) ? line : line.slice(0, commentStart);[m
[32m+[m[32m  match = VAR_ASSIGNMENT.exec(varLine);[m
[32m+[m[32m  if (match !== null) {[m
[32m+[m[32m    return new Variable(match[1], match[2]);[m
[32m+[m[32m  }[m
[32m+[m[32m  return undefined;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mTokenizer.prototype.shift = function () {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    var result = this._shift();[m
[32m+[m[32m    if (result === undefined) {[m
[32m+[m[32m      continue;[m
[32m+[m[32m    }[m
[32m+[m[32m    return result;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction Parser(fileContent) {[m
[32m+[m[32m  this._tokenizer = new Tokenizer(fileContent);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mParser.prototype.parseFile = function () {[m
[32m+[m[32m  var sections = [];[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    var section = this.parseSection();[m
[32m+[m[32m    if (section === null) {[m
[32m+[m[32m      if (this._tokenizer.shift() !== null) {[m
[32m+[m[32m        throw new Error('Unexpected unparsed section of file remains!');[m
[32m+[m[32m      }[m
[32m+[m[32m      return sections;[m
[32m+[m[32m    }[m
[32m+[m[32m    sections.push(section);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mParser.prototype.parseSection = function () {[m
[32m+[m[32m  var section = this._tokenizer.shift();[m
[32m+[m[32m  if (section === null) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m[32m  if (!(section instanceof Section)) {[m
[32m+[m[32m    throw new Error('Expected section heading; got: ' + JSON.stringify(section));[m
[32m+[m[32m  }[m
[32m+[m[32m  var docstring = this._tokenizer.shift();[m
[32m+[m[32m  if (docstring instanceof SectionDocstring) {[m
[32m+[m[32m    section.docstring = docstring;[m
[32m+[m[32m  }[m
[32m+[m[32m  else {[m
[32m+[m[32m    this._tokenizer.unshift(docstring);[m
[32m+[m[32m  }[m
[32m+[m[32m  this.parseSubSections(section);[m
[32m+[m
[32m+[m[32m  return section;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mParser.prototype.parseSubSections = function (section) {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    var subsection = this.parseSubSection();[m
[32m+[m[32m    if (subsection === null) {[m
[32m+[m[32m      if (section.subsections.length === 0) {[m
[32m+[m[32m        // Presume an implicit initial subsection[m
[32m+[m[32m        subsection = new SubSection('');[m
[32m+[m[32m        this.parseVars(subsection);[m
[32m+[m[32m      }[m
[32m+[m[32m      else {[m
[32m+[m[32m        break;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    section.addSubSection(subsection);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (section.subsections.length === 1 && !(section.subsections[0].heading) && section.subsections[0].variables.length === 0) {[m
[32m+[m[32m    // Ignore lone empty implicit subsection[m
[32m+[m[32m    section.subsections = [];[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mParser.prototype.parseSubSection = function () {[m
[32m+[m[32m  var subsection = this._tokenizer.shift();[m
[32m+[m[32m  if (subsection instanceof SubSection) {[m
[32m+[m[32m    this.parseVars(subsection);[m
[32m+[m[32m    return subsection;[m
[32m+[m[32m  }[m
[32m+[m[32m  this._tokenizer.unshift(subsection);[m
[32m+[m[32m  return null;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mParser.prototype.parseVars = function (subsection) {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    var variable = this.parseVar();[m
[32m+[m[32m    if (variable === null) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    subsection.addVar(variable);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mParser.prototype.parseVar = function () {[m
[32m+[m[32m  var docstring = this._tokenizer.shift();[m
[32m+[m[32m  if (!(docstring instanceof VarDocstring)) {[m
[32m+[m[32m    this._tokenizer.unshift(docstring);[m
[32m+[m[32m    docstring = null;[m
[32m+[m[32m  }[m
[32m+[m[32m  var variable = this._tokenizer.shift();[m
[32m+[m[32m  if (variable instanceof Variable) {[m
[32m+[m[32m    variable.docstring = docstring;[m
[32m+[m[32m    return variable;[m
[32m+[m[32m  }[m
[32m+[m[32m  this._tokenizer.unshift(variable);[m
[32m+[m[32m  return null;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32mmodule.exports = Parser;[m
[1mdiff --git a/bower_components/bootstrap/grunt/bs-raw-files-generator.js b/bower_components/bootstrap/grunt/bs-raw-files-generator.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ec8c531[m
[1m--- /dev/null[m
[1m+++ b/bower_components/bootstrap/grunt/bs-raw-files-generator.js[m
[36m@@ -0,0 +1,46 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * Bootstrap Grunt task for generating raw-files.min.js for the Customizer[m
[32m+[m[32m * http://getbootstrap.com[m
[32m+[m[32m * Copyright 2014 Twitter, Inc.[m
[32m+[m[32m * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/* global btoa: true */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m[32mvar fs = require('fs');[m
[32m+[m[32mvar btoa = require('btoa');[m
[32m+[m[32mvar glob = require('glob');[m
[32m+[m
[32m+[m[32mfunction getFiles(type) {[m
[32m+[m[32m  var files = {};[m
[32m+[m[32m  var recursive = (type === 'less');[m
[32m+[m[32m  var globExpr = (recursive ? '/**/*' : '/*');[m
[32m+[m[32m  glob.sync(type + globExpr)[m
[32m+[m[32m    .filter(function (path) {[m
[32m+[m[32m      return type === 'fonts' ? true : new RegExp('\\.' + type + '$').test(path);[m
[32m+[m[32m    })[m
[32m+[m[32m    .forEach(function (fullPath) {[m
[32m+[m[32m      var relativePath = fullPath.replace(/^[^/]+\//, '');[m
[32m+[m[32m      files[relativePath] = (type === 'fonts' ? btoa(fs.readFileSync(fullPath)) : fs.readFileSync(fullPath, 'utf8'));[m
[32m+[m[32m    });[m
[32m+[m[32m  return 'var __' + type + ' = ' + JSON.stringify(files) + '\n';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = function generateRawFilesJs(grunt, banner) {[m
[32m+[m[32m  if (!banner) {[m
[32m+[m[32m    banner = '';[m
[32m+[m[32m  }[m
[32m+[m[32m  var dirs = ['js', 'less', 'fonts'];[m
[32m+[m[32m  var files = banner + dirs.map(getFiles).reduce(function (combined, file) {[m
[32m+[m[32m    return combined + file;[m
[32m+[m[32m  }, '');[m
[32m+[m[32m  var rawFilesJs = 'docs/assets/js/raw-files.min.js';[m
[32m+[m[32m  try {[m
[32m+[m[32m    fs.writeFileSync(rawFilesJs, files);[m
[32m+[m[32m  }[m
[32m+[m[32m  catch (err) {[m
[32m+[m[32m    grunt.fail.warn(err);[m
[32m+[m[32m  }[m
[32m+[m[32m  grunt.log.writeln('File ' + rawFilesJs.cyan + ' created.');[m
[32m+[m[32m};[m
