[32m+[m[32m//[m
[32m+[m[32m// Popovers[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m.popover {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  z-index: @zindex-popover;[m
[32m+[m[32m  display: none;[m
[32m+[m[32m  max-width: @popover-max-width;[m
[32m+[m[32m  padding: 1px;[m
[32m+[m[32m  // Reset font and text properties given new insertion method[m
[32m+[m[32m  font-family: @font-family-base;[m
[32m+[m[32m  font-size: @font-size-base;[m
[32m+[m[32m  font-weight: normal;[m
[32m+[m[32m  line-height: @line-height-base;[m
[32m+[m[32m  text-align: left;[m
[32m+[m[32m  background-color: @popover-bg;[m
[32m+[m[32m  background-clip: padding-box;[m
[32m+[m[32m  border: 1px solid @popover-fallback-border-color;[m
[32m+[m[32m  border: 1px solid @popover-border-color;[m
[32m+[m[32m  border-radius: @border-radius-large;[m
[32m+[m[32m  .box-shadow(0 5px 10px rgba(0,0,0,.2));[m
[32m+[m
[32m+[m[32m  // Overrides for proper insertion[m
[32m+[m[32m  white-space: normal;[m
[32m+[m
[32m+[m[32m  // Offset the popover to account for the popover arrow[m
[32m+[m[32m  &.top     { margin-top: -@popover-arrow-width; }[m
[32m+[m[32m  &.right   { margin-left: @popover-arrow-width; }[m
[32m+[m[32m  &.bottom  { margin-top: @popover-arrow-width; }[m
[32m+[m[32m  &.left    { margin-left: -@popover-arrow-width; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.popover-title {[m
[32m+[m[32m  margin: 0; // reset heading margin[m
[32m+[m[32m  padding: 8px 14px;[m
[32m+[m[32m  font-size: @font-size-base;[m
[32m+[m[32m  background-color: @popover-title-bg;[m
[32m+[m[32m  border-bottom: 1px solid darken(@popover-title-bg, 5%);[m
[32m+[m[32m  border-radius: (@border-radius-large - 1) (@border-radius-large - 1) 0 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.popover-content {[m
[32m+[m[32m  padding: 9px 14px;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Arrows[m
[32m+[m[32m//[m
[32m+[m[32m// .arrow is outer, .arrow:after is inner[m
[32m+[m
[32m+[m[32m.popover > .arrow {[m
[32m+[m[32m  &,[m
[32m+[m[32m  &:after {[m
[32m+[m[32m    position: absolute;[m
[32m+[m[32m    display: block;[m
[32m+[m[32m    width: 0;[m
[32m+[m[32m    height: 0;[m
[32m+[m[32m    border-color: transparent;[m
[32m+[m[32m    border-style: solid;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.popover > .arrow {[m
[32m+[m[32m  border-width: @popover-arrow-outer-width;[m
[32m+[m[32m}[m
[32m+[m[32m.popover > .arrow:after {[m
[32m+[m[32m  border-width: @popover-arrow-width;[m
[32m+[m[32m  content: "";[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.popover {[m
[32m+[m[32m  &.top > .arrow {[m
[32m+[m[32m    left: 50%;[m
[32m+[m[32m    margin-left: -@popover-arrow-outer-width;[m
[32m+[m[32m    border-bottom-width: 0;[m
[32m+[m[32m    border-top-color: @popover-arrow-outer-fallback-color; // IE8 fallback[m
[32m+[m[32m    border-top-color: @popover-arrow-outer-color;[m
[32m+[m[32m    bottom: -@popover-arrow-outer-width;[m
[32m+[m[32m    &:after {[m
[32m+[m[32m      content: " ";[m
[32m+[m[32m      bottom: 1px;[m
[32m+[m[32m      margin-left: -@popover-arrow-width;[m
[32m+[m[32m      border-bottom-width: 0;[m
[32m+[m[32m      border-top-color: @popover-arrow-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  &.right > .arrow {[m
[32m+[m[32m    top: 50%;[m
[32m+[m[32m    left: -@popover-arrow-outer-width;[m
[32m+[m[32m    margin-top: -@popover-arrow-outer-width;[m
[32m+[m[32m    border-left-width: 0;[m
[32m+[m[32m    border-right-color: @popover-arrow-outer-fallback-color; // IE8 fallback[m
[32m+[m[32m    border-right-color: @popover-arrow-outer-color;[m
[32m+[m[32m    &:after {[m
[32m+[m[32m      content: " ";[m
[32m+[m[32m      left: 1px;[m
[32m+[m[32m      bottom: -@popover-arrow-width;[m
[32m+[m[32m      border-left-width: 0;[m
[32m+[m[32m      border-right-color: @popover-arrow-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  &.bottom > .arrow {[m
[32m+[m[32m    left: 50%;[m
[32m+[m[32m    margin-left: -@popover-arrow-outer-width;[m
[32m+[m[32m    border-top-width: 0;[m
[32m+[m[32m    border-bottom-color: @popover-arrow-outer-fallback-color; // IE8 fallback[m
[32m+[m[32m    border-bottom-color: @popover-arrow-outer-color;[m
[32m+[m[32m    top: -@popover-arrow-outer-width;[m
[32m+[m[32m    &:after {[m
[32m+[m[32m      content: " ";[m
[32m+[m[32m      top: 1px;[m
[32m+[m[32m      margin-left: -@popover-arrow-width;[m
[32m+[m[32m      border-top-width: 0;[m
[32m+[m[32m      border-bottom-color: @popover-arrow-color;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &.left > .arrow {[m
[32m+[m[32m    top: 50%;[m
[32m+[m[32m    right: -@popover-arrow-outer-width;[m
[32m+[m[32m    margin-top: -@popover-arrow-outer-width;[m
[32m+[m[32m    border-right-width: 0;[m
