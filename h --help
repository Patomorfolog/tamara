[32m+[m[32mjQuery.fn.extend({[m
[32m+[m	[32mwrapAll: function( html ) {[m
[32m+[m		[32mvar wrap;[m
[32m+[m
[32m+[m		[32mif ( jQuery.isFunction( html ) ) {[m
[32m+[m			[32mreturn this.each(function( i ) {[m
[32m+[m				[32mjQuery( this ).wrapAll( html.call(this, i) );[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( this[ 0 ] ) {[m
[32m+[m
[32m+[m			[32m// The elements to wrap the target around[m
[32m+[m			[32mwrap = jQuery( html, this[ 0 ].ownerDocument ).eq( 0 ).clone( true );[m
[32m+[m
[32m+[m			[32mif ( this[ 0 ].parentNode ) {[m
[32m+[m				[32mwrap.insertBefore( this[ 0 ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mwrap.map(function() {[m
[32m+[m				[32mvar elem = this;[m
[32m+[m
[32m+[m				[32mwhile ( elem.firstElementChild ) {[m
[32m+[m					[32melem = elem.firstElementChild;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn elem;[m
[32m+[m			[32m}).append( this );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mwrapInner: function( html ) {[m
[32m+[m		[32mif ( jQuery.isFunction( html ) ) {[m
[32m+[m			[32mreturn this.each(function( i ) {[m
[32m+[m				[32mjQuery( this ).wrapInner( html.call(this, i) );[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.each(function() {[m
[32m+[m			[32mvar self = jQuery( this ),[m
[32m+[m				[32mcontents = self.contents();[m
[32m+[m
[32m+[m			[32mif ( contents.length ) {[m
[32m+[m				[32mcontents.wrapAll( html );[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mself.append( html );[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mwrap: function( html ) {[m
[32m+[m		[32mvar isFunction = jQuery.isFunction( html );[m
[32m+[m
[32m+[m		[32mreturn this.each(function( i ) {[m
[32m+[m			[32mjQuery( this ).wrapAll( isFunction ? html.call(this, i) : html );[m
[32m+[m		[32m});[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32munwrap: function() {[m
[32m+[m		[32mreturn this.parent().each(function() {[m
[32m+[m			[32mif ( !jQuery.nodeName( this, "body" ) ) {[m
[32m+[m				[32mjQuery( this ).replaceWith( this.childNodes );[m
[32m+[m			[32m}[m
[32m+[m		[32m}).end();[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mreturn jQuery;[m
[32m+[m[32m});[m
