[32m+[m[32mdefine([[m
[32m+[m	[32m"./core",[m
[32m+[m	[32m"./core/access",[m
[32m+[m	[32m"./css"[m
[32m+[m[32m], function( jQuery, access ) {[m
[32m+[m
[32m+[m[32m// Create innerHeight, innerWidth, height, width, outerHeight and outerWidth methods[m
[32m+[m[32mjQuery.each( { Height: "height", Width: "width" }, function( name, type ) {[m
[32m+[m	[32mjQuery.each( { padding: "inner" + name, content: type, "": "outer" + name }, function( defaultExtra, funcName ) {[m
[32m+[m		[32m// Margin is only for outerHeight, outerWidth[m
[32m+[m		[32mjQuery.fn[ funcName ] = function( margin, value ) {[m
[32m+[m			[32mvar chainable = arguments.length && ( defaultExtra || typeof margin !== "boolean" ),[m
[32m+[m				[32mextra = defaultExtra || ( margin === true || value === true ? "margin" : "border" );[m
[32m+[m
[32m+[m			[32mreturn access( this, function( elem, type, value ) {[m
[32m+[m				[32mvar doc;[m
[32m+[m
[32m+[m				[32mif ( jQuery.isWindow( elem ) ) {[m
[32m+[m					[32m// As of 5/8/2012 this will yield incorrect results for Mobile Safari, but there[m
[32m+[m					[32m// isn't a whole lot we can do. See pull request at this URL for discussion:[m
[32m+[m					[32m// https://github.com/jquery/jquery/pull/764[m
[32m+[m					[32mreturn elem.document.documentElement[ "client" + name ];[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Get document width or height[m
[32m+[m				[32mif ( elem.nodeType === 9 ) {[m
[32m+[m					[32mdoc = elem.documentElement;[m
[32m+[m
[32m+[m					[32m// Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height],[m
[32m+[m					[32m// whichever is greatest[m
[32m+[m					[32mreturn Math.max([m
[32m+[m						[32melem.body[ "scroll" + name ], doc[ "scroll" + name ],[m
[32m+[m						[32melem.body[ "offset" + name ], doc[ "offset" + name ],[m
[32m+[m						[32mdoc[ "client" + name ][m
[32m+[m					[32m);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn value === undefined ?[m
[32m+[m					[32m// Get width or height on the element, requesting but not forcing parseFloat[m
[32m+[m					[32mjQuery.css( elem, type, extra ) :[m
[32m+[m
[32m+[m					[32m// Set width or height on the element[m
[32m+[m					[32mjQuery.style( elem, type, value, extra );[m
[32m+[m			[32m}, type, chainable ? margin : undefined, chainable, null );[m
[32m+[m		[32m};[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mreturn jQuery;[m
[32m+[m[32m});[m
