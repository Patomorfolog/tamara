[32m+[m[32m    background-color: transparent;[m
[32m+[m[32m    .box-shadow(none);[m
[32m+[m[32m  }[m
[32m+[m[32m  &,[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus,[m
[32m+[m[32m  &:active {[m
