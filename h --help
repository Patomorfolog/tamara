[36m@@ -0,0 +1,39 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"../core",[m
[32m+[m	[32m"./var/rsingleTag",[m
[32m+[m	[32m"../manipulation" // buildFragment[m
[32m+[m[32m], function( jQuery, rsingleTag ) {[m
[32m+[m
[32m+[m[32m// data: string of html[m
[32m+[m[32m// context (optional): If specified, the fragment will be created in this context, defaults to document[m
[32m+[m[32m// keepScripts (optional): If true, will include scripts passed in the html string[m
[32m+[m[32mjQuery.parseHTML = function( data, context, keepScripts ) {[m
[32m+[m	[32mif ( !data || typeof data !== "string" ) {[m
[32m+[m		[32mreturn null;[m
[32m+[m	[32m}[m
[32m+[m	[32mif ( typeof context === "boolean" ) {[m
[32m+[m		[32mkeepScripts = context;[m
[32m+[m		[32mcontext = false;[m
[32m+[m	[32m}[m
[32m+[m	[32mcontext = context || document;[m
[32m+[m
[32m+[m	[32mvar parsed = rsingleTag.exec( data ),[m
[32m+[m		[32mscripts = !keepScripts && [];[m
[32m+[m
[32m+[m	[32m// Single tag[m
[32m+[m	[32mif ( parsed ) {[m
[32m+[m		[32mreturn [ context.createElement( parsed[1] ) ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mparsed = jQuery.buildFragment( [ data ], context, scripts );[m
[32m+[m
[32m+[m	[32mif ( scripts && scripts.length ) {[m
[32m+[m		[32mjQuery( scripts ).remove();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn jQuery.merge( [], parsed.childNodes );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreturn jQuery.parseHTML;[m
[32m+[m
[32m+[m[32m});[m
[1mdiff --git a/bower_components/jquery/src/core/ready.js b/bower_components/jquery/src/core/ready.js[m
[1mnew file mode 100644[m
[1mindex 0000000..db1a6e6[m
[1m--- /dev/null[m
[1m+++ b/bower_components/jquery/src/core/ready.js[m
[36m@@ -0,0 +1,97 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"../core",[m
[32m+[m	[32m"../core/init",[m
[32m+[m	[32m"../deferred"[m
[32m+[m[32m], function( jQuery ) {[m
[32m+[m
[32m+[m[32m// The deferred used on DOM ready[m
[32m+[m[32mvar readyList;[m
[32m+[m
[32m+[m[32mjQuery.fn.ready = function( fn ) {[m
[32m+[m	[32m// Add the callback[m
[32m+[m	[32mjQuery.ready.promise().done( fn );[m
[32m+[m
[32m+[m	[32mreturn this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.extend({[m
[32m+[m	[32m// Is the DOM ready to be used? Set to true once it occurs.[m
[32m+[m	[32misReady: false,[m
[32m+[m
[32m+[m	[32m// A counter to track how many items to wait for before[m
[32m+[m	[32m// the ready event fires. See #6781[m
[32m+[m	[32mreadyWait: 1,[m
[32m+[m
[32m+[m	[32m// Hold (or release) the ready event[m
[32m+[m	[32mholdReady: function( hold ) {[m
[32m+[m		[32mif ( hold ) {[m
[32m+[m			[32mjQuery.readyWait++;[m
[32m+[m		[32m} else {[m
[32m+[m			[32mjQuery.ready( true );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Handle when the DOM is ready[m
[32m+[m	[32mready: function( wait ) {[m
[32m+[m
[32m+[m		[32m// Abort if there are pending holds or we're already ready[m
[32m+[m		[32mif ( wait === true ? --jQuery.readyWait : jQuery.isReady ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remember that the DOM is ready[m
[32m+[m		[32mjQuery.isReady = true;[m
[32m+[m
[32m+[m		[32m// If a normal DOM Ready event fired, decrement, and wait if need be[m
[32m+[m		[32mif ( wait !== true && --jQuery.readyWait > 0 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If there are functions bound, to execute[m
[32m+[m		[32mreadyList.resolveWith( document, [ jQuery ] );[m
[32m+[m
[32m+[m		[32m// Trigger any bound ready events[m
[32m+[m		[32mif ( jQuery.fn.triggerHandler ) {[m
[32m+[m			[32mjQuery( document ).triggerHandler( "ready" );[m
[32m+[m			[32mjQuery( document ).off( "ready" );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * The ready event handler and self cleanup method[m
[32m+[m[32m */[m
[32m+[m[32mfunction completed() {[m
[32m+[m	[32mdocument.removeEventListener( "DOMContentLoaded", completed, false );[m
[32m+[m	[32mwindow.removeEventListener( "load", completed, false );[m
[32m+[m	[32mjQuery.ready();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.ready.promise = function( obj ) {[m
[32m+[m	[32mif ( !readyList ) {[m
[32m+[m
[32m+[m		[32mreadyList = jQuery.Deferred();[m
[32m+[m
[32m+[m		[32m// Catch cases where $(document).ready() is called after the browser event has already occurred.[m
[32m+[m		[32m// We once tried to use readyState "interactive" here, but it caused issues like the one[m
[32m+[m		[32m// discovered by ChrisS here: http://bugs.jquery.com/ticket/12282#comment:15[m
[32m+[m		[32mif ( document.readyState === "complete" ) {[m
[32m+[m			[32m// Handle it asynchronously to allow scripts the opportunity to delay ready[m
[32m+[m			[32msetTimeout( jQuery.ready );[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// Use the handy event callback[m
[32m+[m			[32mdocument.addEventListener( "DOMContentLoaded", completed, false );[m
[32m+[m
[32m+[m			[32m// A fallback to window.onload, that will always work[m
[32m+[m			[32mwindow.addEventListener( "load", completed, false );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn readyList.promise( obj );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Kick off the DOM ready check even if the user does not[m
[32m+[m[32mjQuery.ready.promise();[m
[32m+[m
[32m+[m[32m});[m
