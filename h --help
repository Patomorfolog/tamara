[32m+[m[32m//[m
[32m+[m[32m// Carousel[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m// Wrapper for the slide container and indicators[m
[32m+[m[32m.carousel {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.carousel-inner {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m  width: 100%;[m
[32m+[m
[32m+[m[32m  > .item {[m
[32m+[m[32m    display: none;[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    .transition(.6s ease-in-out left);[m
[32m+[m
[32m+[m[32m    // Account for jankitude on images[m
[32m+[m[32m    > img,[m
[32m+[m[32m    > a > img {[m
[32m+[m[32m      &:extend(.img-responsive);[m
[32m+[m[32m      line-height: 1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // WebKit CSS3 transforms for supported devices[m
[32m+[m[32m    @media all and (transform-3d), (-webkit-transform-3d) {[m
[32m+[m[32m      .transition-transform(~'0.6s ease-in-out');[m
[32m+[m[32m      .backface-visibility(~'hidden');[m
[32m+[m[32m      .perspective(1000);[m
[32m+[m
[32m+[m[32m      &.next,[m
[32m+[m[32m      &.active.right {[m
[32m+[m[32m        .translate3d(100%, 0, 0);[m
[32m+[m[32m        left: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m      &.prev,[m
[32m+[m[32m      &.active.left {[m
[32m+[m[32m        .translate3d(-100%, 0, 0);[m
[32m+[m[32m        left: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m      &.next.left,[m
[32m+[m[32m      &.prev.right,[m
[32m+[m[32m      &.active {[m
[32m+[m[32m        .translate3d(0, 0, 0);[m
[32m+[m[32m        left: 0;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active,[m
[32m+[m[32m  > .next,[m
[32m+[m[32m  > .prev {[m
[32m+[m[32m    display: block;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active {[m
[32m+[m[32m    left: 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .next,[m
[32m+[m[32m  > .prev {[m
[32m+[m[32m    position: absolute;[m
[32m+[m[32m    top: 0;[m
[32m+[m[32m    width: 100%;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .next {[m
[32m+[m[32m    left: 100%;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .prev {[m
[32m+[m[32m    left: -100%;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .next.left,[m
[32m+[m[32m  > .prev.right {[m
[32m+[m[32m    left: 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > .active.left {[m
[32m+[m[32m    left: -100%;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .active.right {[m
[32m+[m[32m    left: 100%;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Left/right controls for nav[m
[32m+[m[32m// ---------------------------[m
[32m+[m
[32m+[m[32m.carousel-control {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  top: 0;[m
[32m+[m[32m  left: 0;[m
[32m+[m[32m  bottom: 0;[m
[32m+[m[32m  width: @carousel-control-width;[m
[32m+[m[32m  .opacity(@carousel-control-opacity);[m
[32m+[m[32m  font-size: @carousel-control-font-size;[m
[32m+[m[32m  color: @carousel-control-color;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  text-shadow: @carousel-text-shadow;[m
[32m+[m[32m  // We can't have this transition here because WebKit cancels the carousel[m
[32m+[m[32m  // animation if you trip this while in the middle of another animation.[m
[32m+[m
[32m+[m[32m  // Set gradients for backgrounds[m
[32m+[m[32m  &.left {[m
[32m+[m[32m    #gradient > .horizontal(@start-color: rgba(0,0,0,.5); @end-color: rgba(0,0,0,.0001));[m
[32m+[m[32m  }[m
[32m+[m[32m  &.right {[m
[32m+[m[32m    left: auto;[m
[32m+[m[32m    right: 0;[m
[32m+[m[32m    #gradient > .horizontal(@start-color: rgba(0,0,0,.0001); @end-color: rgba(0,0,0,.5));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Hover/focus state[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    outline: 0;[m
[32m+[m[32m    color: @carousel-control-color;[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m    .opacity(.9);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Toggles[m
[32m+[m[32m  .icon-prev,[m
[32m+[m[32m  .icon-next,[m
[32m+[m[32m  .glyphicon-chevron-left,[m
[32m+[m[32m  .glyphicon-chevron-right {[m
[32m+[m[32m    position: absolute;[m
[32m+[m[32m    top: 50%;[m
[32m+[m[32m    z-index: 5;[m
[32m+[m[32m    display: inline-block;[m
[32m+[m[32m  }[m
[32m+[m[32m  .icon-prev,[m
[32m+[m[32m  .glyphicon-chevron-left {[m
[32m+[m[32m    left: 50%;[m
[32m+[m[32m    margin-left: -10px;[m
[32m+[m[32m  }[m
[32m+[m[32m  .icon-next,[m
[32m+[m[32m  .glyphicon-chevron-right {[m
[32m+[m[32m    right: 50%;[m
[32m+[m[32m    margin-right: -10px;[m
[32m+[m[32m  }[m
[32m+[m[32m  .icon-prev,[m
[32m+[m[32m  .icon-next {[m
[32m+[m[32m    width:  20px;[m
[32m+[m[32m    height: 20px;[m
[32m+[m[32m    margin-top: -10px;[m
[32m+[m[32m    line-height: 1;[m
[32m+[m[32m    font-family: serif;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  .icon-prev {[m
[32m+[m[32m    &:before {[m
[32m+[m[32m      content: '\2039';// SINGLE LEFT-POINTING ANGLE QUOTATION MARK (U+2039)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  .icon-next {[m
[32m+[m[32m    &:before {[m
[32m+[m[32m      content: '\203a';// SINGLE RIGHT-POINTING ANGLE QUOTATION MARK (U+203A)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional indicator pips[m
[32m+[m[32m//[m
[32m+[m[32m// Add an unordered list with the following class and add a list item for each[m
[32m+[m[32m// slide your carousel holds.[m
[32m+[m
[32m+[m[32m.carousel-indicators {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  bottom: 10px;[m
[32m+[m[32m  left: 50%;[m
[32m+[m[32m  z-index: 15;[m
[32m+[m[32m  width: 60%;[m
[32m+[m[32m  margin-left: -30%;[m
[32m+[m[32m  padding-left: 0;[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m  text-align: center;[m
[32m+[m
[32m+[m[32m  li {[m
[32m+[m[32m    display: inline-block;[m
[32m+[m[32m    width:  10px;[m
[32m+[m[32m    height: 10px;[m
[32m+[m[32m    margin: 1px;[m
[32m+[m[32m    text-indent: -999px;[m
[32m+[m[32m    border: 1px solid @carousel-indicator-border-color;[m
[32m+[m[32m    border-radius: 10px;[m
[32m+[m[32m    cursor: pointer;[m
[32m+[m
[32m+[m[32m    // IE8-9 hack for event handling[m
[32m+[m[32m    //[m
[32m+[m[32m    // Internet Explorer 8-9 does not support clicks on elements without a set[m
[32m+[m[32m    // `background-color`. We cannot use `filter` since that's not viewed as a[m
[32m+[m[32m    // background color by the browser. Thus, a hack is needed.[m
[32m+[m[32m    // See https://developer.mozilla.org/en-US/docs/Web/Events/click#Internet_Explorer[m
[32m+[m[32m    //[m
[32m+[m[32m    // For IE8, we set solid black as it doesn't support `rgba()`. For IE9, we[m
[32m+[m[32m    // set alpha transparency for the best results possible.[m
[32m+[m[32m    background-color: #000 \9; // IE8[m
[32m+[m[32m    background-color: rgba(0,0,0,0); // IE9[m
[32m+[m[32m  }[m
[32m+[m[32m  .active {[m
[32m+[m[32m    margin: 0;[m
[32m+[m[32m    width:  12px;[m
[32m+[m[32m    height: 12px;[m
[32m+[m[32m    background-color: @carousel-indicator-active-bg;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional captions[m
[32m+[m[32m// -----------------------------[m
[32m+[m[32m// Hidden by default for smaller viewports[m
[32m+[m[32m.carousel-caption {[m
[32m+[m[32m  position: absolute;[m
[32m+[m[32m  left: 15%;[m
[32m+[m[32m  right: 15%;[m
[32m+[m[32m  bottom: 20px;[m
[32m+[m[32m  z-index: 10;[m
[32m+[m[32m  padding-top: 20px;[m
[32m+[m[32m  padding-bottom: 20px;[m
[32m+[m[32m  color: @carousel-caption-color;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  text-shadow: @carousel-text-shadow;[m
[32m+[m[32m  & .btn {[m
[32m+[m[32m    text-shadow: none; // No shadow for button elements in carousel-caption[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
