[32m+[m[32m  }[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus {[m
[32m+[m[32m    color: @link-hover-color;[m
[32m+[m[32m    text-decoration: @link-hover-decoration;[m
[32m+[m[32m    background-color: transparent;[m
[32m+[m[32m  }[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus {[m
[32m+[m[32m      color: @btn-link-disabled-color;[m
[32m+[m[32m      text-decoration: none;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
