[32m+[m[32m    var fileContent = fs.readFileSync(filePath, { encoding: 'utf8' });[m
[32m+[m[32m    var parser = new BsLessdocParser(fileContent);[m
[32m+[m[32m    return { sections: parser.parseFile() };[m
[32m+[m[32m  };[m
