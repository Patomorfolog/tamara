[32m+[m[32m//[m
[32m+[m[32m// Button groups[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// Make the div behave like a button[m
[32m+[m[32m.btn-group,[m
[32m+[m[32m.btn-group-vertical {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  vertical-align: middle; // match .btn alignment given font-size hack above[m
[32m+[m[32m  > .btn {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    // Bring the "active" button to the front[m
[32m+[m[32m    &:hover,[m
[32m+[m[32m    &:focus,[m
[32m+[m[32m    &:active,[m
[32m+[m[32m    &.active {[m
[32m+[m[32m      z-index: 2;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Prevent double borders when buttons are next to each other[m
[32m+[m[32m.btn-group {[m
[32m+[m[32m  .btn + .btn,[m
[32m+[m[32m  .btn + .btn-group,[m
[32m+[m[32m  .btn-group + .btn,[m
[32m+[m[32m  .btn-group + .btn-group {[m
[32m+[m[32m    margin-left: -1px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Optional: Group multiple button groups together for a toolbar[m
[32m+[m[32m.btn-toolbar {[m
[32m+[m[32m  margin-left: -5px; // Offset the first child's margin[m
[32m+[m[32m  &:extend(.clearfix all);[m
[32m+[m
[32m+[m[32m  .btn-group,[m
[32m+[m[32m  .input-group {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m  }[m
[32m+[m[32m  > .btn,[m
[32m+[m[32m  > .btn-group,[m
[32m+[m[32m  > .input-group {[m
[32m+[m[32m    margin-left: 5px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.btn-group > .btn:not(:first-child):not(:last-child):not(.dropdown-toggle) {[m
[32m+[m[32m  border-radius: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Set corners individual because sometimes a single button can be in a .btn-group and we need :first-child and :last-child to both match[m
[32m+[m[32m.btn-group > .btn:first-child {[m
[32m+[m[32m  margin-left: 0;[m
[32m+[m[32m  &:not(:last-child):not(.dropdown-toggle) {[m
[32m+[m[32m    .border-right-radius(0);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m// Need .dropdown-toggle since :last-child doesn't apply given a .dropdown-menu immediately after it[m
[32m+[m[32m.btn-group > .btn:last-child:not(:first-child),[m
[32m+[m[32m.btn-group > .dropdown-toggle:not(:first-child) {[m
[32m+[m[32m  .border-left-radius(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Custom edits for including btn-groups within btn-groups (useful for including dropdown buttons within a btn-group)[m
[32m+[m[32m.btn-group > .btn-group {[m
[32m+[m[32m  float: left;[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-group:not(:first-child):not(:last-child) > .btn {[m
[32m+[m[32m  border-radius: 0;[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-group:first-child:not(:last-child) {[m
[32m+[m[32m  > .btn:last-child,[m
[32m+[m[32m  > .dropdown-toggle {[m
[32m+[m[32m    .border-right-radius(0);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.btn-group > .btn-group:last-child:not(:first-child) > .btn:first-child {[m
[32m+[m[32m  .border-left-radius(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// On active and open, don't show outline[m
[32m+[m[32m.btn-group .dropdown-toggle:active,[m
[32m+[m[32m.btn-group.open .dropdown-toggle {[m
[32m+[m[32m  outline: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Sizing[m
[32m+[m[32m//[m
[32m+[m[32m// Remix the default button sizing classes into new ones for easier manipulation.[m
[32m+[m
