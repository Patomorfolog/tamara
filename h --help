[32m+[m[32m// Image Mixins[m
[32m+[m[32m// - Responsive image[m
[32m+[m[32m// - Retina image[m
[32m+[m
[32m+[m
[32m+[m[32m// Responsive image[m
[32m+[m[32m//[m
[32m+[m[32m// Keep images from scaling beyond the width of their parents.[m
[32m+[m[32m.img-responsive(@display: block) {[m
[32m+[m[32m  display: @display;[m
[32m+[m[32m  max-width: 100%; // Part 1: Set a maximum relative to the parent[m
[32m+[m[32m  height: auto; // Part 2: Scale the height according to the width, otherwise you get stretching[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Retina image[m
[32m+[m[32m//[m
[32m+[m[32m// Short retina mixin for setting background-image and -size. Note that the[m
[32m+[m[32m// spelling of `min--moz-device-pixel-ratio` is intentional.[m
[32m+[m[32m.img-retina(@file-1x; @file-2x; @width-1x; @height-1x) {[m
[32m+[m[32m  background-image: url("@{file-1x}");[m
[32m+[m
[32m+[m[32m  @media[m
[32m+[m[32m  only screen and (-webkit-min-device-pixel-ratio: 2),[m
[32m+[m[32m  only screen and (   min--moz-device-pixel-ratio: 2),[m
[32m+[m[32m  only screen and (     -o-min-device-pixel-ratio: 2/1),[m
[32m+[m[32m  only screen and (        min-device-pixel-ratio: 2),[m
[32m+[m[32m  only screen and (                min-resolution: 192dpi),[m
[32m+[m[32m  only screen and (                min-resolution: 2dppx) {[m
[32m+[m[32m    background-image: url("@{file-2x}");[m
[32m+[m[32m    background-size: @width-1x @height-1x;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
