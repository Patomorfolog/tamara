[32m+[m[32m//[m
[32m+[m[32m// Component animations[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m// Heads up![m
[32m+[m[32m//[m
[32m+[m[32m// We don't use the `.opacity()` mixin here since it causes a bug with text[m
[32m+[m[32m// fields in IE7-8. Source: https://github.com/twbs/bootstrap/pull/3552.[m
[32m+[m
[32m+[m[32m.fade {[m
[32m+[m[32m  opacity: 0;[m
[32m+[m[32m  .transition(opacity .15s linear);[m
[32m+[m[32m  &.in {[m
[32m+[m[32m    opacity: 1;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.collapse {[m
[32m+[m[32m  display: none;[m
[32m+[m
[32m+[m[32m  &.in      { display: block; }[m
[32m+[m[32m  tr&.in    { display: table-row; }[m
[32m+[m[32m  tbody&.in { display: table-row-group; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.collapsing {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  height: 0;[m
[32m+[m[32m  overflow: hidden;[m
[32m+[m[32m  .transition-property(~"height, visibility");[m
[32m+[m[32m  .transition-duration(.35s);[m
[32m+[m[32m  .transition-timing-function(ease);[m
[32m+[m[32m}[m
