[36m@@ -0,0 +1,122 @@[m
[32m+[m[32m// Grid system[m
[32m+[m[32m//[m
[32m+[m[32m// Generate semantic grid columns with these mixins.[m
[32m+[m
[32m+[m[32m// Centered container element[m
[32m+[m[32m.container-fixed(@gutter: @grid-gutter-width) {[m
[32m+[m[32m  margin-right: auto;[m
[32m+[m[32m  margin-left: auto;[m
[32m+[m[32m  padding-left:  (@gutter / 2);[m
[32m+[m[32m  padding-right: (@gutter / 2);[m
[32m+[m[32m  &:extend(.clearfix all);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Creates a wrapper for a series of columns[m
[32m+[m[32m.make-row(@gutter: @grid-gutter-width) {[m
[32m+[m[32m  margin-left:  (@gutter / -2);[m
[32m+[m[32m  margin-right: (@gutter / -2);[m
[32m+[m[32m  &:extend(.clearfix all);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the extra small columns[m
[32m+[m[32m.make-xs-column(@columns; @gutter: @grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  float: left;[m
[32m+[m[32m  width: percentage((@columns / @grid-columns));[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  (@gutter / 2);[m
[32m+[m[32m  padding-right: (@gutter / 2);[m
[32m+[m[32m}[m
[32m+[m[32m.make-xs-column-offset(@columns) {[m
[32m+[m[32m  margin-left: percentage((@columns / @grid-columns));[m
[32m+[m[32m}[m
[32m+[m[32m.make-xs-column-push(@columns) {[m
[32m+[m[32m  left: percentage((@columns / @grid-columns));[m
[32m+[m[32m}[m
[32m+[m[32m.make-xs-column-pull(@columns) {[m
[32m+[m[32m  right: percentage((@columns / @grid-columns));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the small columns[m
[32m+[m[32m.make-sm-column(@columns; @gutter: @grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  (@gutter / 2);[m
[32m+[m[32m  padding-right: (@gutter / 2);[m
[32m+[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    float: left;[m
[32m+[m[32m    width: percentage((@columns / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.make-sm-column-offset(@columns) {[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    margin-left: percentage((@columns / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.make-sm-column-push(@columns) {[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    left: percentage((@columns / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.make-sm-column-pull(@columns) {[m
[32m+[m[32m  @media (min-width: @screen-sm-min) {[m
[32m+[m[32m    right: percentage((@columns / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate the medium columns[m
[32m+[m[32m.make-md-column(@columns; @gutter: @grid-gutter-width) {[m
[32m+[m[32m  position: relative;[m
[32m+[m[32m  min-height: 1px;[m
[32m+[m[32m  padding-left:  (@gutter / 2);[m
[32m+[m[32m  padding-right: (@gutter / 2);[m
[32m+[m
[32m+[m[32m  @media (min-width: @screen-md-min) {[m
[32m+[m[32m    float: left;[m
