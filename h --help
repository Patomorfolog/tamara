[32m+[m[32mdefine([[m
[32m+[m	[32m"../core",[m
[32m+[m	[32m"../var/support"[m
[32m+[m[32m], function( jQuery, support ) {[m
[32m+[m
[32m+[m[32m(function() {[m
[32m+[m	[32mvar pixelPositionVal, boxSizingReliableVal,[m
[32m+[m		[32mdocElem = document.documentElement,[m
[32m+[m		[32mcontainer = document.createElement( "div" ),[m
[32m+[m		[32mdiv = document.createElement( "div" );[m
[32m+[m
[32m+[m	[32mif ( !div.style ) {[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Support: IE9-11+[m
[32m+[m	[32m// Style of cloned element affects source element cloned (#8908)[m
[32m+[m	[32mdiv.style.backgroundClip = "content-box";[m
[32m+[m	[32mdiv.cloneNode( true ).style.backgroundClip = "";[m
[32m+[m	[32msupport.clearCloneStyle = div.style.backgroundClip === "content-box";[m
[32m+[m
[32m+[m	[32mcontainer.style.cssText = "border:0;width:0;height:0;top:0;left:-9999px;margin-top:1px;" +[m
[32m+[m		[32m"position:absolute";[m
[32m+[m	[32mcontainer.appendChild( div );[m
[32m+[m
[32m+[m	[32m// Executing both pixelPosition & boxSizingReliable tests require only one layout[m
[32m+[m	[32m// so they're executed at the same time to save the second computation.[m
[32m+[m	[32mfunction computePixelPositionAndBoxSizingReliable() {[m
[32m+[m		[32mdiv.style.cssText =[m
[32m+[m			[32m// Support: Firefox<29, Android 2.3[m
[32m+[m			[32m// Vendor-prefix box-sizing[m
[32m+[m			[32m"-webkit-box-sizing:border-box;-moz-box-sizing:border-box;" +[m
[32m+[m			[32m"box-sizing:border-box;display:block;margin-top:1%;top:1%;" +[m
[32m+[m			[32m"border:1px;padding:1px;width:4px;position:absolute";[m
[32m+[m		[32mdiv.innerHTML = "";[m
[32m+[m		[32mdocElem.appendChild( container );[m
[32m+[m
[32m+[m		[32mvar divStyle = window.getComputedStyle( div, null );[m
[32m+[m		[32mpixelPositionVal = divStyle.top !== "1%";[m
[32m+[m		[32mboxSizingReliableVal = divStyle.width === "4px";[m
[32m+[m
[32m+[m		[32mdocElem.removeChild( container );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Support: node.js jsdom[m
[32m+[m	[32m// Don't assume that getComputedStyle is a property of the global object[m
[32m+[m	[32mif ( window.getComputedStyle ) {[m
[32m+[m		[32mjQuery.extend( support, {[m
[32m+[m			[32mpixelPosition: function() {[m
[32m+[m
[32m+[m				[32m// This test is executed only once but we still do memoizing[m
[32m+[m				[32m// since we can use the boxSizingReliable pre-computing.[m
[32m+[m				[32m// No need to check if the test was already performed, though.[m
[32m+[m				[32mcomputePixelPositionAndBoxSizingReliable();[m
[32m+[m				[32mreturn pixelPositionVal;[m
[32m+[m			[32m},[m
[32m+[m			[32mboxSizingReliable: function() {[m
[32m+[m				[32mif ( boxSizingReliableVal == null ) {[m
[32m+[m					[32mcomputePixelPositionAndBoxSizingReliable();[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn boxSizingReliableVal;[m
[32m+[m			[32m},[m
[32m+[m			[32mreliableMarginRight: function() {[m
[32m+[m
[32m+[m				[32m// Support: Android 2.3[m
[32m+[m				[32m// Check if div with explicit width and no margin-right incorrectly[m
[32m+[m				[32m// gets computed margin-right based on width of container. (#3333)[m
[32m+[m				[32m// WebKit Bug 13343 - getComputedStyle returns wrong value for margin-right[m
[32m+[m				[32m// This support function is only executed once so no memoizing is needed.[m
[32m+[m				[32mvar ret,[m
[32m+[m					[32mmarginDiv = div.appendChild( document.createElement( "div" ) );[m
[32m+[m
[32m+[m				[32m// Reset CSS: box-sizing; display; margin; border; padding[m
[32m+[m				[32mmarginDiv.style.cssText = div.style.cssText =[m
[32m+[m					[32m// Support: Firefox<29, Android 2.3[m
[32m+[m					[32m// Vendor-prefix box-sizing[m
[32m+[m					[32m"-webkit-box-sizing:content-box;-moz-box-sizing:content-box;" +[m
[32m+[m					[32m"box-sizing:content-box;display:block;margin:0;border:0;padding:0";[m
[32m+[m				[32mmarginDiv.style.marginRight = marginDiv.style.width = "0";[m
[32m+[m				[32mdiv.style.width = "1px";[m
[32m+[m				[32mdocElem.appendChild( container );[m
[32m+[m
[32m+[m				[32mret = !parseFloat( window.getComputedStyle( marginDiv, null ).marginRight );[m
[32m+[m
[32m+[m				[32mdocElem.removeChild( container );[m
[32m+[m				[32mdiv.removeChild( marginDiv );[m
[32m+[m
[32m+[m				[32mreturn ret;[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m[32m})();[m
[32m+[m
[32m+[m[32mreturn support;[m
[32m+[m
[32m+[m[32m});[m
[1mdiff --git a/bower_components/jquery/src/css/swap.js b/bower_components/jquery/src/css/swap.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ce16435[m
[1m--- /dev/null[m
[1m+++ b/bower_components/jquery/src/css/swap.js[m
[36m@@ -0,0 +1,28 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"../core"[m
[32m+[m[32m], function( jQuery ) {[m
[32m+[m
[32m+[m[32m// A method for quickly swapping in/out CSS properties to get correct calculations.[m
[32m+[m[32mjQuery.swap = function( elem, options, callback, args ) {[m
[32m+[m	[32mvar ret, name,[m
[32m+[m		[32mold = {};[m
[32m+[m
[32m+[m	[32m// Remember the old values, and insert the new ones[m
[32m+[m	[32mfor ( name in options ) {[m
[32m+[m		[32mold[ name ] = elem.style[ name ];[m
[32m+[m		[32melem.style[ name ] = options[ name ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mret = callback.apply( elem, args || [] );[m
[32m+[m
[32m+[m	[32m// Revert the old values[m
[32m+[m	[32mfor ( name in options ) {[m
[32m+[m		[32melem.style[ name ] = old[ name ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn ret;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreturn jQuery.swap;[m
[32m+[m
[32m+[m[32m});[m
