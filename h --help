[32m+[m[32mdefine([[m
[32m+[m	[32m"../core",[m
[32m+[m	[32m"../queue",[m
[32m+[m	[32m"../effects" // Delay is optional because of this dependency[m
[32m+[m[32m], function( jQuery ) {[m
[32m+[m
[32m+[m[32m// Based off of the plugin by Clint Helfers, with permission.[m
[32m+[m[32m// http://blindsignals.com/index.php/2009/07/jquery-delay/[m
[32m+[m[32mjQuery.fn.delay = function( time, type ) {[m
[32m+[m	[32mtime = jQuery.fx ? jQuery.fx.speeds[ time ] || time : time;[m
[32m+[m	[32mtype = type || "fx";[m
[32m+[m
[32m+[m	[32mreturn this.queue( type, function( next, hooks ) {[m
[32m+[m		[32mvar timeout = setTimeout( next, time );[m
[32m+[m		[32mhooks.stop = function() {[m
[32m+[m			[32mclearTimeout( timeout );[m
[32m+[m		[32m};[m
[32m+[m	[32m});[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreturn jQuery.fn.delay;[m
[32m+[m[32m});[m
