[32m+[m
[32m+[m[32m// Base styles[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m[32m.btn {[m
[32m+[m[32m  display: inline-block;[m
[32m+[m[32m  margin-bottom: 0; // For input.btn[m
[32m+[m[32m  font-weight: @btn-font-weight;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  vertical-align: middle;[m
[32m+[m[32m  touch-action: manipulation;[m
[32m+[m[32m  cursor: pointer;[m
[32m+[m[32m  background-image: none; // Reset unusual Firefox-on-Android default style; see https://github.com/necolas/normalize.css/issues/214[m
[32m+[m[32m  border: 1px solid transparent;[m
[32m+[m[32m  white-space: nowrap;[m
[32m+[m[32m  .button-size(@padding-base-vertical; @padding-base-horizontal; @font-size-base; @line-height-base; @border-radius-base);[m
[32m+[m[32m  .user-select(none);[m
[32m+[m
[32m+[m[32m  &,[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active {[m
[32m+[m[32m    &:focus,[m
[32m+[m[32m    &.focus {[m
[32m+[m[32m      .tab-focus();[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &:hover,[m
[32m+[m[32m  &:focus,[m
[32m+[m[32m  &.focus {[m
[32m+[m[32m    color: @btn-default-color;[m
[32m+[m[32m    text-decoration: none;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &:active,[m
[32m+[m[32m  &.active {[m
[32m+[m[32m    outline: 0;[m
[32m+[m[32m    background-image: none;[m
[32m+[m[32m    .box-shadow(inset 0 3px 5px rgba(0,0,0,.125));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  &.disabled,[m
[32m+[m[32m  &[disabled],[m
[32m+[m[32m  fieldset[disabled] & {[m
[32m+[m[32m    cursor: @cursor-disabled;[m
[32m+[m[32m    pointer-events: none; // Future-proof disabling of clicks[m
[32m+[m[32m    .opacity(.65);[m
[32m+[m[32m    .box-shadow(none);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
