[1mdiff --git a/bower_components/jquery/src/deferred.js b/bower_components/jquery/src/deferred.js[m
[1mnew file mode 100644[m
[1mindex 0000000..98f9c31[m
[1m--- /dev/null[m
[1m+++ b/bower_components/jquery/src/deferred.js[m
[36m@@ -0,0 +1,149 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"./core",[m
[32m+[m	[32m"./var/slice",[m
[32m+[m	[32m"./callbacks"[m
[32m+[m[32m], function( jQuery, slice ) {[m
[32m+[m
[32m+[m[32mjQuery.extend({[m
[32m+[m
[32m+[m	[32mDeferred: function( func ) {[m
[32m+[m		[32mvar tuples = [[m
[32m+[m				[32m// action, add listener, listener list, final state[m
[32m+[m				[32m[ "resolve", "done", jQuery.Callbacks("once memory"), "resolved" ],[m
[32m+[m				[32m[ "reject", "fail", jQuery.Callbacks("once memory"), "rejected" ],[m
[32m+[m				[32m[ "notify", "progress", jQuery.Callbacks("memory") ][m
[32m+[m			[32m],[m
[32m+[m			[32mstate = "pending",[m
[32m+[m			[32mpromise = {[m
[32m+[m				[32mstate: function() {[m
[32m+[m					[32mreturn state;[m
[32m+[m				[32m},[m
[32m+[m				[32malways: function() {[m
[32m+[m					[32mdeferred.done( arguments ).fail( arguments );[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m},[m
[32m+[m				[32mthen: function( /* fnDone, fnFail, fnProgress */ ) {[m
[32m+[m					[32mvar fns = arguments;[m
[32m+[m					[32mreturn jQuery.Deferred(function( newDefer ) {[m
[32m+[m						[32mjQuery.each( tuples, function( i, tuple ) {[m
[32m+[m							[32mvar fn = jQuery.isFunction( fns[ i ] ) && fns[ i ];[m
[32m+[m							[32m// deferred[ done | fail | progress ] for forwarding actions to newDefer[m
[32m+[m							[32mdeferred[ tuple[1] ](function() {[m
[32m+[m								[32mvar returned = fn && fn.apply( this, arguments );[m
[32m+[m								[32mif ( returned && jQuery.isFunction( returned.promise ) ) {[m
[32m+[m									[32mreturned.promise()[m
[32m+[m										[32m.done( newDefer.resolve )[m
[32m+[m										[32m.fail( newDefer.reject )[m
[32m+[m										[32m.progress( newDefer.notify );[m
[32m+[m								[32m} else {[m
[32m+[m									[32mnewDefer[ tuple[ 0 ] + "With" ]( this === promise ? newDefer.promise() : this, fn ? [ returned ] : arguments );[m
[32m+[m								[32m}[m
[32m+[m							[32m});[m
[32m+[m						[32m});[m
[32m+[m						[32mfns = null;[m
[32m+[m					[32m}).promise();[m
[32m+[m				[32m},[m
[32m+[m				[32m// Get a promise for this deferred[m
[32m+[m				[32m// If obj is provided, the promise aspect is added to the object[m
[32m+[m				[32mpromise: function( obj ) {[m
[32m+[m					[32mreturn obj != null ? jQuery.extend( obj, promise ) : promise;[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m			[32mdeferred = {};[m
[32m+[m
[32m+[m		[32m// Keep pipe for back-compat[m
[32m+[m		[32mpromise.pipe = promise.then;[m
[32m+[m
[32m+[m		[32m// Add list-specific methods[m
[32m+[m		[32mjQuery.each( tuples, function( i, tuple ) {[m
[32m+[m			[32mvar list = tuple[ 2 ],[m
[32m+[m				[32mstateString = tuple[ 3 ];[m
[32m+[m
[32m+[m			[32m// promise[ done | fail | progress ] = list.add[m
[32m+[m			[32mpromise[ tuple[1] ] = list.add;[m
[32m+[m
[32m+[m			[32m// Handle state[m
[32m+[m			[32mif ( stateString ) {[m
[32m+[m				[32mlist.add(function() {[m
[32m+[m					[32m// state = [ resolved | rejected ][m
[32m+[m					[32mstate = stateString;[m
[32m+[m
[32m+[m				[32m// [ reject_list | resolve_list ].disable; progress_list.lock[m
[32m+[m				[32m}, tuples[ i ^ 1 ][ 2 ].disable, tuples[ 2 ][ 2 ].lock );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// deferred[ resolve | reject | notify ][m
[32m+[m			[32mdeferred[ tuple[0] ] = function() {[m
[32m+[m				[32mdeferred[ tuple[0] + "With" ]( this === deferred ? promise : this, arguments );[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m};[m
[32m+[m			[32mdeferred[ tuple[0] + "With" ] = list.fireWith;[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32m// Make the deferred a promise[m
[32m+[m		[32mpromise.promise( deferred );[m
[32m+[m
[32m+[m		[32m// Call given func if any[m
[32m+[m		[32mif ( func ) {[m
[32m+[m			[32mfunc.call( deferred, deferred );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// All done![m
[32m+[m		[32mreturn deferred;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Deferred helper[m
[32m+[m	[32mwhen: function( subordinate /* , ..., subordinateN */ ) {[m
[32m+[m		[32mvar i = 0,[m
[32m+[m			[32mresolveValues = slice.call( arguments ),[m
[32m+[m			[32mlength = resolveValues.length,[m
[32m+[m
[32m+[m			[32m// the count of uncompleted subordinates[m
[32m+[m			[32mremaining = length !== 1 || ( subordinate && jQuery.isFunction( subordinate.promise ) ) ? length : 0,[m
[32m+[m
[32m+[m			[32m// the master Deferred. If resolveValues consist of only a single Deferred, just use that.[m
[32m+[m			[32mdeferred = remaining === 1 ? subordinate : jQuery.Deferred(),[m
[32m+[m
[32m+[m			[32m// Update function for both resolve and progress values[m
[32m+[m			[32mupdateFunc = function( i, contexts, values ) {[m
[32m+[m				[32mreturn function( value ) {[m
[32m+[m					[32mcontexts[ i ] = this;[m
[32m+[m					[32mvalues[ i ] = arguments.length > 1 ? slice.call( arguments ) : value;[m
[32m+[m					[32mif ( values === progressValues ) {[m
[32m+[m						[32mdeferred.notifyWith( contexts, values );[m
[32m+[m					[32m} else if ( !( --remaining ) ) {[m
[32m+[m						[32mdeferred.resolveWith( contexts, values );[m
[32m+[m					[32m}[m
[32m+[m				[32m};[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32mprogressValues, progressContexts, resolveContexts;[m
[32m+[m
[32m+[m		[32m// Add listeners to Deferred subordinates; treat others as resolved[m
[32m+[m		[32mif ( length > 1 ) {[m
[32m+[m			[32mprogressValues = new Array( length );[m
[32m+[m			[32mprogressContexts = new Array( length );[m
[32m+[m			[32mresolveContexts = new Array( length );[m
[32m+[m			[32mfor ( ; i < length; i++ ) {[m
[32m+[m				[32mif ( resolveValues[ i ] && jQuery.isFunction( resolveValues[ i ].promise ) ) {[m
[32m+[m					[32mresolveValues[ i ].promise()[m
[32m+[m						[32m.done( updateFunc( i, resolveContexts, resolveValues ) )[m
[32m+[m						[32m.fail( deferred.reject )[m
[32m+[m						[32m.progress( updateFunc( i, progressContexts, progressValues ) );[m
[32m+[m				[32m} else {[m
[32m+[m					[32m--remaining;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If we're not waiting on anything, resolve the master[m
[32m+[m		[32mif ( !remaining ) {[m
[32m+[m			[32mdeferred.resolveWith( resolveContexts, resolveValues );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn deferred.promise();[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mreturn jQuery;[m
[32m+[m[32m});[m
