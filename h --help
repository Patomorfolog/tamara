[32m+[m[32mdefine([[m
[32m+[m	[32m"../core"[m
[32m+[m[32m], function( jQuery ) {[m
[32m+[m
[32m+[m[32m// Multifunctional method to get and set values of a collection[m
[32m+[m[32m// The value/s can optionally be executed if it's a function[m
[32m+[m[32mvar access = jQuery.access = function( elems, fn, key, value, chainable, emptyGet, raw ) {[m
[32m+[m	[32mvar i = 0,[m
[32m+[m		[32mlen = elems.length,[m
[32m+[m		[32mbulk = key == null;[m
[32m+[m
[32m+[m	[32m// Sets many values[m
[32m+[m	[32mif ( jQuery.type( key ) === "object" ) {[m
[32m+[m		[32mchainable = true;[m
[32m+[m		[32mfor ( i in key ) {[m
[32m+[m			[32mjQuery.access( elems, fn, i, key[i], true, emptyGet, raw );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m// Sets one value[m
[32m+[m	[32m} else if ( value !== undefined ) {[m
[32m+[m		[32mchainable = true;[m
[32m+[m
[32m+[m		[32mif ( !jQuery.isFunction( value ) ) {[m
[32m+[m			[32mraw = true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( bulk ) {[m
[32m+[m			[32m// Bulk operations run against the entire set[m
[32m+[m			[32mif ( raw ) {[m
[32m+[m				[32mfn.call( elems, value );[m
[32m+[m				[32mfn = null;[m
[32m+[m
[32m+[m			[32m// ...except when executing function values[m
[32m+[m			[32m} else {[m
[32m+[m				[32mbulk = fn;[m
[32m+[m				[32mfn = function( elem, key, value ) {[m
[32m+[m					[32mreturn bulk.call( jQuery( elem ), value );[m
[32m+[m				[32m};[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( fn ) {[m
[32m+[m			[32mfor ( ; i < len; i++ ) {[m
[32m+[m				[32mfn( elems[i], key, raw ? value : value.call( elems[i], i, fn( elems[i], key ) ) );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn chainable ?[m
[32m+[m		[32melems :[m
[32m+[m
[32m+[m		[32m// Gets[m
[32m+[m		[32mbulk ?[m
[32m+[m			[32mfn.call( elems ) :[m
[32m+[m			[32mlen ? fn( elems[0], key ) : emptyGet;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreturn access;[m
[32m+[m
[32m+[m[32m});[m
