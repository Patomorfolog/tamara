[32m+[m[32m(function( window ) {[m
[32m+[m
[32m+[m[32mvar i,[m
[32m+[m	[32msupport,[m
[32m+[m	[32mExpr,[m
[32m+[m	[32mgetText,[m
[32m+[m	[32misXML,[m
