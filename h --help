[1mnew file mode 100644[m
[1mindex 0000000..f3b3929[m
[1m--- /dev/null[m
[1m+++ b/bower_components/bootstrap/less/mixins/grid-framework.less[m
[36m@@ -0,0 +1,91 @@[m
[32m+[m[32m// Framework grid generation[m
[32m+[m[32m//[m
[32m+[m[32m// Used only by Bootstrap to generate the correct number of grid classes given[m
[32m+[m[32m// any value of `@grid-columns`.[m
[32m+[m
[32m+[m[32m.make-grid-columns() {[m
[32m+[m[32m  // Common styles for all sizes of grid columns, widths 1-12[m
[32m+[m[32m  .col(@index) { // initial[m
[32m+[m[32m    @item: ~".col-xs-@{index}, .col-sm-@{index}, .col-md-@{index}, .col-lg-@{index}";[m
[32m+[m[32m    .col((@index + 1), @item);[m
[32m+[m[32m  }[m
[32m+[m[32m  .col(@index, @list) when (@index =< @grid-columns) { // general; "=<" isn't a typo[m
[32m+[m[32m    @item: ~".col-xs-@{index}, .col-sm-@{index}, .col-md-@{index}, .col-lg-@{index}";[m
[32m+[m[32m    .col((@index + 1), ~"@{list}, @{item}");[m
[32m+[m[32m  }[m
[32m+[m[32m  .col(@index, @list) when (@index > @grid-columns) { // terminal[m
[32m+[m[32m    @{list} {[m
[32m+[m[32m      position: relative;[m
[32m+[m[32m      // Prevent columns from collapsing when empty[m
[32m+[m[32m      min-height: 1px;[m
[32m+[m[32m      // Inner gutter via padding[m
[32m+[m[32m      padding-left:  (@grid-gutter-width / 2);[m
[32m+[m[32m      padding-right: (@grid-gutter-width / 2);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  .col(1); // kickstart it[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.float-grid-columns(@class) {[m
[32m+[m[32m  .col(@index) { // initial[m
[32m+[m[32m    @item: ~".col-@{class}-@{index}";[m
[32m+[m[32m    .col((@index + 1), @item);[m
[32m+[m[32m  }[m
[32m+[m[32m  .col(@index, @list) when (@index =< @grid-columns) { // general[m
[32m+[m[32m    @item: ~".col-@{class}-@{index}";[m
[32m+[m[32m    .col((@index + 1), ~"@{list}, @{item}");[m
[32m+[m[32m  }[m
[32m+[m[32m  .col(@index, @list) when (@index > @grid-columns) { // terminal[m
[32m+[m[32m    @{list} {[m
[32m+[m[32m      float: left;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  .col(1); // kickstart it[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.calc-grid-column(@index, @class, @type) when (@type = width) and (@index > 0) {[m
[32m+[m[32m  .col-@{class}-@{index} {[m
[32m+[m[32m    width: percentage((@index / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.calc-grid-column(@index, @class, @type) when (@type = push) and (@index > 0) {[m
[32m+[m[32m  .col-@{class}-push-@{index} {[m
[32m+[m[32m    left: percentage((@index / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.calc-grid-column(@index, @class, @type) when (@type = push) and (@index = 0) {[m
[32m+[m[32m  .col-@{class}-push-0 {[m
[32m+[m[32m    left: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.calc-grid-column(@index, @class, @type) when (@type = pull) and (@index > 0) {[m
[32m+[m[32m  .col-@{class}-pull-@{index} {[m
[32m+[m[32m    right: percentage((@index / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.calc-grid-column(@index, @class, @type) when (@type = pull) and (@index = 0) {[m
[32m+[m[32m  .col-@{class}-pull-0 {[m
[32m+[m[32m    right: auto;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m.calc-grid-column(@index, @class, @type) when (@type = offset) {[m
[32m+[m[32m  .col-@{class}-offset-@{index} {[m
[32m+[m[32m    margin-left: percentage((@index / @grid-columns));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Basic looping in LESS[m
[32m+[m[32m.loop-grid-columns(@index, @class, @type) when (@index >= 0) {[m
[32m+[m[32m  .calc-grid-column(@index, @class, @type);[m
[32m+[m[32m  // next iteration[m
[32m+[m[32m  .loop-grid-columns((@index - 1), @class, @type);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Create grid for specific class[m
[32m+[m[32m.make-grid(@class) {[m
[32m+[m[32m  .float-grid-columns(@class);[m
[32m+[m[32m  .loop-grid-columns(@grid-columns, @class, width);[m
[32m+[m[32m  .loop-grid-columns(@grid-columns, @class, pull);[m
[32m+[m[32m  .loop-grid-columns(@grid-columns, @class, push);[m
[32m+[m[32m  .loop-grid-columns(@grid-columns, @class, offset);[m
[32m+[m[32m}[m
[1mdiff --git a/bower_components/bootstrap/less/mixins/grid.less b/bower_components/bootstrap/less/mixins/grid.less[m
[1mnew file mode 100644[m
[1mindex 0000000..cae5eaf[m
[1m--- /dev/null[m
