[32m+[m
[32m+[m[32m.alert {[m
[32m+[m[32m  padding: @alert-padding;[m
[32m+[m[32m  margin-bottom: @line-height-computed;[m
[32m+[m[32m  border: 1px solid transparent;[m
[32m+[m[32m  border-radius: @alert-border-radius;[m
[32m+[m
[32m+[m[32m  // Headings for larger alerts[m
[32m+[m[32m  h4 {[m
[32m+[m[32m    margin-top: 0;[m
[32m+[m[32m    // Specified for the h4 to prevent conflicts of changing @headings-color[m
[32m+[m[32m    color: inherit;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Provide class for links that match alerts[m
[32m+[m[32m  .alert-link {[m
[32m+[m[32m    font-weight: @alert-link-font-weight;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Improve alignment and spacing of inner content[m
[32m+[m[32m  > p,[m
[32m+[m[32m  > ul {[m
[32m+[m[32m    margin-bottom: 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  > p + p {[m
[32m+[m[32m    margin-top: 5px;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Dismissible alerts[m
[32m+[m[32m//[m
[32m+[m[32m// Expand the right padding and account for the close button's positioning.[m
[32m+[m
[32m+[m[32m.alert-dismissable, // The misspelled .alert-dismissable was deprecated in 3.2.0.[m
[32m+[m[32m.alert-dismissible {[m
[32m+[m[32m  padding-right: (@alert-padding + 20);[m
[32m+[m
[32m+[m[32m  // Adjust close link position[m
[32m+[m[32m  .close {[m
[32m+[m[32m    position: relative;[m
[32m+[m[32m    top: -2px;[m
[32m+[m[32m    right: -21px;[m
[32m+[m[32m    color: inherit;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Alternate styles[m
[32m+[m[32m//[m
[32m+[m[32m// Generate contextual modifier classes for colorizing the alert.[m
[32m+[m
[32m+[m[32m.alert-success {[m
[32m+[m[32m  .alert-variant(@alert-success-bg; @alert-success-border; @alert-success-text);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.alert-info {[m
[32m+[m[32m  .alert-variant(@alert-info-bg; @alert-info-border; @alert-info-text);[m
[32m+[m[32m}[m
[32m+[m
