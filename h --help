[32m+[m	[32msupport.optSelected = opt.selected;[m
[32m+[m
[32m+[m	[32m// Support: Android<=2.3[m
[32m+[m	[32m// Options inside disabled selects are incorrectly marked as disabled[m
[32m+[m	[32mselect.disabled = true;[m
[32m+[m	[32msupport.optDisabled = !opt.disabled;[m
[32m+[m
[32m+[m	[32m// Support: IE<=11+[m
[32m+[m	[32m// An input loses its value after becoming a radio[m
[32m+[m	[32minput = document.createElement( "input" );[m
[32m+[m	[32minput.value = "t";[m
[32m+[m	[32minput.type = "radio";[m
[32m+[m	[32msupport.radioValue = input.value === "t";[m
[32m+[m[32m})();[m
[32m+[m
[32m+[m[32mreturn support;[m
[32m+[m
[32m+[m[32m});[m
