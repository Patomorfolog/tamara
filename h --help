[1mdiff --git a/bower_components/jquery/src/data.js b/bower_components/jquery/src/data.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f9af9ae[m
[1m--- /dev/null[m
[1m+++ b/bower_components/jquery/src/data.js[m
[36m@@ -0,0 +1,178 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"./core",[m
[32m+[m	[32m"./var/rnotwhite",[m
[32m+[m	[32m"./core/access",[m
[32m+[m	[32m"./data/var/data_priv",[m
[32m+[m	[32m"./data/var/data_user"[m
[32m+[m[32m], function( jQuery, rnotwhite, access, data_priv, data_user ) {[m
[32m+[m
[32m+[m[32m//	Implementation Summary[m
[32m+[m[32m//[m
[32m+[m[32m//	1. Enforce API surface and semantic compatibility with 1.9.x branch[m
[32m+[m[32m//	2. Improve the module's maintainability by reducing the storage[m
[32m+[m[32m//		paths to a single mechanism.[m
[32m+[m[32m//	3. Use the same single mechanism to support "private" and "user" data.[m
[32m+[m[32m//	4. _Never_ expose "private" data to user code (TODO: Drop _data, _removeData)[m
[32m+[m[32m//	5. Avoid exposing implementation details on user objects (eg. expando properties)[m
[32m+[m[32m//	6. Provide a clear path for implementation upgrade to WeakMap in 2014[m
[32m+[m
[32m+[m[32mvar rbrace = /^(?:\{[\w\W]*\}|\[[\w\W]*\])$/,[m
[32m+[m	[32mrmultiDash = /([A-Z])/g;[m
[32m+[m
[32m+[m[32mfunction dataAttr( elem, key, data ) {[m
[32m+[m	[32mvar name;[m
[32m+[m
[32m+[m	[32m// If nothing was found internally, try to fetch any[m
[32m+[m	[32m// data from the HTML5 data-* attribute[m
[32m+[m	[32mif ( data === undefined && elem.nodeType === 1 ) {[m
[32m+[m		[32mname = "data-" + key.replace( rmultiDash, "-$1" ).toLowerCase();[m
[32m+[m		[32mdata = elem.getAttribute( name );[m
[32m+[m
[32m+[m		[32mif ( typeof data === "string" ) {[m
[32m+[m			[32mtry {[m
[32m+[m				[32mdata = data === "true" ? true :[m
[32m+[m					[32mdata === "false" ? false :[m
[32m+[m					[32mdata === "null" ? null :[m
[32m+[m					[32m// Only convert to a number if it doesn't change the string[m
[32m+[m					[32m+data + "" === data ? +data :[m
[32m+[m					[32mrbrace.test( data ) ? jQuery.parseJSON( data ) :[m
[32m+[m					[32mdata;[m
[32m+[m			[32m} catch( e ) {}[m
[32m+[m
[32m+[m			[32m// Make sure we set the data so it isn't changed later[m
[32m+[m			[32mdata_user.set( elem, key, data );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mdata = undefined;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn data;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.extend({[m
[32m+[m	[32mhasData: function( elem ) {[m
[32m+[m		[32mreturn data_user.hasData( elem ) || data_priv.hasData( elem );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mdata: function( elem, name, data ) {[m
[32m+[m		[32mreturn data_user.access( elem, name, data );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveData: function( elem, name ) {[m
[32m+[m		[32mdata_user.remove( elem, name );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// TODO: Now that all calls to _data and _removeData have been replaced[m
[32m+[m	[32m// with direct calls to data_priv methods, these can be deprecated.[m
[32m+[m	[32m_data: function( elem, name, data ) {[m
[32m+[m		[32mreturn data_priv.access( elem, name, data );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m_removeData: function( elem, name ) {[m
[32m+[m		[32mdata_priv.remove( elem, name );[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mjQuery.fn.extend({[m
[32m+[m	[32mdata: function( key, value ) {[m
[32m+[m		[32mvar i, name, data,[m
[32m+[m			[32melem = this[ 0 ],[m
[32m+[m			[32mattrs = elem && elem.attributes;[m
[32m+[m
[32m+[m		[32m// Gets all values[m
[32m+[m		[32mif ( key === undefined ) {[m
[32m+[m			[32mif ( this.length ) {[m
[32m+[m				[32mdata = data_user.get( elem );[m
[32m+[m
[32m+[m				[32mif ( elem.nodeType === 1 && !data_priv.get( elem, "hasDataAttrs" ) ) {[m
[32m+[m					[32mi = attrs.length;[m
[32m+[m					[32mwhile ( i-- ) {[m
[32m+[m
[32m+[m						[32m// Support: IE11+[m
[32m+[m						[32m// The attrs elements can be null (#14894)[m
[32m+[m						[32mif ( attrs[ i ] ) {[m
[32m+[m							[32mname = attrs[ i ].name;[m
[32m+[m							[32mif ( name.indexOf( "data-" ) === 0 ) {[m
[32m+[m								[32mname = jQuery.camelCase( name.slice(5) );[m
[32m+[m								[32mdataAttr( elem, name, data[ name ] );[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mdata_priv.set( elem, "hasDataAttrs", true );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn data;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Sets multiple values[m
[32m+[m		[32mif ( typeof key === "object" ) {[m
[32m+[m			[32mreturn this.each(function() {[m
[32m+[m				[32mdata_user.set( this, key );[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn access( this, function( value ) {[m
[32m+[m			[32mvar data,[m
[32m+[m				[32mcamelKey = jQuery.camelCase( key );[m
[32m+[m
[32m+[m			[32m// The calling jQuery object (element matches) is not empty[m
[32m+[m			[32m// (and therefore has an element appears at this[ 0 ]) and the[m
[32m+[m			[32m// `value` parameter was not undefined. An empty jQuery object[m
[32m+[m			[32m// will result in `undefined` for elem = this[ 0 ] which will[m
[32m+[m			[32m// throw an exception if an attempt to read a data cache is made.[m
[32m+[m			[32mif ( elem && value === undefined ) {[m
[32m+[m				[32m// Attempt to get data from the cache[m
[32m+[m				[32m// with the key as-is[m
[32m+[m				[32mdata = data_user.get( elem, key );[m
[32m+[m				[32mif ( data !== undefined ) {[m
[32m+[m					[32mreturn data;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Attempt to get data from the cache[m
[32m+[m				[32m// with the key camelized[m
[32m+[m				[32mdata = data_user.get( elem, camelKey );[m
[32m+[m				[32mif ( data !== undefined ) {[m
[32m+[m					[32mreturn data;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Attempt to "discover" the data in[m
[32m+[m				[32m// HTML5 custom data-* attrs[m
[32m+[m				[32mdata = dataAttr( elem, camelKey, undefined );[m
[32m+[m				[32mif ( data !== undefined ) {[m
[32m+[m					[32mreturn data;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// We tried really hard, but the data doesn't exist.[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Set the data...[m
[32m+[m			[32mthis.each(function() {[m
[32m+[m				[32m// First, attempt to store a copy or reference of any[m
[32m+[m				[32m// data that might've been store with a camelCased key.[m
[32m+[m				[32mvar data = data_user.get( this, camelKey );[m
[32m+[m
[32m+[m				[32m// For HTML5 data-* attribute interop, we have to[m
[32m+[m				[32m// store property names with dashes in a camelCase form.[m
[32m+[m				[32m// This might not apply to all properties...*[m
[32m+[m				[32mdata_user.set( this, camelKey, value );[m
[32m+[m
[32m+[m				[32m// *... In the case of properties that might _actually_[m
[32m+[m				[32m// have dashes, we need to also store a copy of that[m
[32m+[m				[32m// unchanged property.[m
[32m+[m				[32mif ( key.indexOf("-") !== -1 && data !== undefined ) {[m
[32m+[m					[32mdata_user.set( this, key, value );[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m		[32m}, null, value, arguments.length > 1, null, true );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveData: function( key ) {[m
[32m+[m		[32mreturn this.each(function() {[m
[32m+[m			[32mdata_user.remove( this, key );[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mreturn jQuery;[m
[32m+[m[32m});[m
[1mdiff --git a/bower_components/jquery/src/data/Data.js b/bower_components/jquery/src/data/Data.js[m
[1mnew file mode 100644[m
[1mindex 0000000..85afd64[m
[1m--- /dev/null[m
[1m+++ b/bower_components/jquery/src/data/Data.js[m
[36m@@ -0,0 +1,181 @@[m
[32m+[m[32mdefine([[m
[32m+[m	[32m"../core",[m
[32m+[m	[32m"../var/rnotwhite",[m
[32m+[m	[32m"./accepts"[m
[32m+[m[32m], function( jQuery, rnotwhite ) {[m
[32m+[m
[32m+[m[32mfunction Data() {[m
[32m+[m	[32m// Support: Android<4,[m
[32m+[m	[32m// Old WebKit does not have Object.preventExtensions/freeze method,[m
[32m+[m	[32m// return new empty object instead with no [[set]] accessor[m
[32m+[m	[32mObject.defineProperty( this.cache = {}, 0, {[m
[32m+[m		[32mget: function() {[m
[32m+[m			[32mreturn {};[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mthis.expando = jQuery.expando + Data.uid++;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mData.uid = 1;[m
[32m+[m[32mData.accepts = jQuery.acceptData;[m
[32m+[m
[32m+[m[32mData.prototype = {[m
[32m+[m	[32mkey: function( owner ) {[m
[32m+[m		[32m// We can accept data for non-element nodes in modern browsers,[m
[32m+[m		[32m// but we should not, see #8335.[m
[32m+[m		[32m// Always return the key for a frozen object.[m
[32m+[m		[32mif ( !Data.accepts( owner ) ) {[m
[32m+[m			[32mreturn 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar descriptor = {},[m
[32m+[m			[32m// Check if the owner object already has a cache key[m
[32m+[m			[32munlock = owner[ this.expando ];[m
[32m+[m
[32m+[m		[32m// If not, create one[m
[32m+[m		[32mif ( !unlock ) {[m
[32m+[m			[32munlock = Data.uid++;[m
[32m+[m
[32m+[m			[32m// Secure it in a non-enumerable, non-writable property[m
[32m+[m			[32mtry {[m
[32m+[m				[32mdescriptor[ this.expando ] = { value: unlock };[m
[32m+[m				[32mObject.defineProperties( owner, descriptor );[m
[32m+[m
[32m+[m			[32m// Support: Android<4[m
[32m+[m			[32m// Fallback to a less secure definition[m
[32m+[m			[32m} catch ( e ) {[m
[32m+[m				[32mdescriptor[ this.expando ] = unlock;[m
[32m+[m				[32mjQuery.extend( owner, descriptor );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Ensure the cache object[m
[32m+[m		[32mif ( !this.cache[ unlock ] ) {[m
[32m+[m			[32mthis.cache[ unlock ] = {};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn unlock;[m
[32m+[m	[32m},[m
[32m+[m	[32mset: function( owner, data, value ) {[m
[32m+[m		[32mvar prop,[m
[32m+[m			[32m// There may be an unlock assigned to this node,[m
[32m+[m			[32m// if there is no entry for this "owner", create one inline[m
[32m+[m			[32m// and set the unlock as though an owner entry had always existed[m
[32m+[m			[32munlock = this.key( owner ),[m
[32m+[m			[32mcache = this.cache[ unlock ];[m
[32m+[m
[32m+[m		[32m// Handle: [ owner, key, value ] args[m
[32m+[m		[32mif ( typeof data === "string" ) {[m
[32m+[m			[32mcache[ data ] = value;[m
[32m+[m
[32m+[m		[32m// Handle: [ owner, { properties } ] args[m
[32m+[m		[32m} else {[m
[32m+[m			[32m// Fresh assignments by object are shallow copied[m
[32m+[m			[32mif ( jQuery.isEmptyObject( cache ) ) {[m
[32m+[m				[32mjQuery.extend( this.cache[ unlock ], data );[m
[32m+[m			[32m// Otherwise, copy the properties one-by-one to the cache object[m
[32m+[m			[32m} else {[m
[32m+[m				[32mfor ( prop in data ) {[m
[32m+[m					[32mcache[ prop ] = data[ prop ];[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn cache;[m
[32m+[m	[32m},[m
[32m+[m	[32mget: function( owner, key ) {[m
[32m+[m		[32m// Either a valid cache is found, or will be created.[m
[32m+[m		[32m// New caches will be created and the unlock returned,[m
[32m+[m		[32m// allowing direct access to the newly created[m
[32m+[m		[32m// empty data object. A valid owner object must be provided.[m
[32m+[m		[32mvar cache = this.cache[ this.key( owner ) ];[m
[32m+[m
[32m+[m		[32mreturn key === undefined ?[m
[32m+[m			[32mcache : cache[ key ];[m
[32m+[m	[32m},[m
[32m+[m	[32maccess: function( owner, key, value ) {[m
[32m+[m		[32mvar stored;[m
[32m+[m		[32m// In cases where either:[m
[32m+[m		[32m//[m
[32m+[m		[32m//   1. No key was specified[m
[32m+[m		[32m//   2. A string key was specified, but no value provided[m
[32m+[m		[32m//[m
[32m+[m		[32m// Take the "read" path and allow the get method to determine[m
[32m+[m		[32m// which value to return, respectively either:[m
[32m+[m		[32m//[m
[32m+[m		[32m//   1. The entire cache object[m
[32m+[m		[32m//   2. The data stored at the key[m
[32m+[m		[32m//[m
[32m+[m		[32mif ( key === undefined ||[m
[32m+[m				[32m((key && typeof key === "string") && value === undefined) ) {[m
[32m+[m
[32m+[m			[32mstored = this.get( owner, key );[m
[32m+[m
[32m+[m			[32mreturn stored !== undefined ?[m
[32m+[m				[32mstored : this.get( owner, jQuery.camelCase(key) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// [*]When the key is not a string, or both a key and value[m
[32m+[m		[32m// are specified, set or extend (existing objects) with either:[m
[32m+[m		[32m//[m
[32m+[m		[32m//   1. An object of properties[m
[32m+[m		[32m//   2. A key and value[m
[32m+[m		[32m//[m
[32m+[m		[32mthis.set( owner, key, value );[m
[32m+[m
[32m+[m		[32m// Since the "set" path can have two possible entry points[m
[32m+[m		[32m// return the expected data based on which path was taken[*][m
[32m+[m		[32mreturn value !== undefined ? value : key;[m
[32m+[m	[32m},[m
[32m+[m	[32mremove: function( owner, key ) {[m
[32m+[m		[32mvar i, name, camel,[m
[32m+[m			[32munlock = this.key( owner ),[m
[32m+[m			[32mcache = this.cache[ unlock ];[m
[32m+[m
[32m+[m		[32mif ( key === undefined ) {[m
[32m+[m			[32mthis.cache[ unlock ] = {};[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m			[32m// Support array or space separated string of keys[m
[32m+[m			[32mif ( jQuery.isArray( key ) ) {[m
[32m+[m				[32m// If "name" is an array of keys...[m
[32m+[m				[32m// When data is initially created, via ("key", "val") signature,[m
[32m+[m				[32m// keys will be converted to camelCase.[m
[32m+[m				[32m// Since there is no way to tell _how_ a key was added, remove[m
[32m+[m				[32m// both plain key and camelCase key. #12786[m
[32m+[m				[32m// This will only penalize the array argument path.[m
[32m+[m				[32mname = key.concat( key.map( jQuery.camelCase ) );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mcamel = jQuery.camelCase( key );[m
[32m+[m				[32m// Try the string as a key before any manipulation[m
[32m+[m				[32mif ( key in cache ) {[m
[32m+[m					[32mname = [ key, camel ];[m
[32m+[m				[32m} else {[m
[32m+[m					[32m// If a key with the spaces exists, use it.[m
[32m+[m					[32m// Otherwise, create an array by matching non-whitespace[m
[32m+[m					[32mname = camel;[m
[32m+[m					[32mname = name in cache ?[m
[32m+[m						[32m[ name ] : ( name.match( rnotwhite ) || [] );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mi = name.length;[m
[32m+[m			[32mwhile ( i-- ) {[m
[32m+[m				[32mdelete cache[ name[ i ] ];[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32mhave its original line endings in your working directory.
