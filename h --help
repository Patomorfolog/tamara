[32m+[m[32m//[m
[32m+[m[32m// Pager pagination[m
[32m+[m[32m// --------------------------------------------------[m
[32m+[m
[32m+[m
[32m+[m[32m.pager {[m
[32m+[m[32m  padding-left: 0;[m
[32m+[m[32m  margin: @line-height-computed 0;[m
[32m+[m[32m  list-style: none;[m
[32m+[m[32m  text-align: center;[m
[32m+[m[32m  &:extend(.clearfix all);[m
[32m+[m[32m  li {[m
[32m+[m[32m    display: inline;[m
[32m+[m[32m    > a,[m
[32m+[m[32m    > span {[m
[32m+[m[32m      display: inline-block;[m
[32m+[m[32m      padding: 5px 14px;[m
[32m+[m[32m      background-color: @pager-bg;[m
[32m+[m[32m      border: 1px solid @pager-border;[m
[32m+[m[32m      border-radius: @pager-border-radius;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    > a:hover,[m
[32m+[m[32m    > a:focus {[m
[32m+[m[32m      text-decoration: none;[m
[32m+[m[32m      background-color: @pager-hover-bg;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .next {[m
[32m+[m[32m    > a,[m
[32m+[m[32m    > span {[m
[32m+[m[32m      float: right;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .previous {[m
[32m+[m[32m    > a,[m
[32m+[m[32m    > span {[m
[32m+[m[32m      float: left;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  .disabled {[m
[32m+[m[32m    > a,[m
[32m+[m[32m    > a:hover,[m
[32m+[m[32m    > a:focus,[m
[32m+[m[32m    > span {[m
[32m+[m[32m      color: @pager-disabled-color;[m
[32m+[m[32m      background-color: @pager-bg;[m
[32m+[m[32m      cursor: @cursor-disabled;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
