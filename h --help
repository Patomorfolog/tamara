[32m+[m[32m    htmllint: {[m
[32m+[m[32m      options: {[m
[32m+[m[32m        ignore: [[m
[32m+[m[32m          'Attribute "autocomplete" not allowed on element "button" at this point.',[m
[32m+[m[32m          'Attribute "autocomplete" not allowed on element "input" at this point.',[m
[32m+[m[32m          'Element "img" is missing required attribute "src".'[m
[32m+[m[32m        ][m
[32m+[m[32m      },[m
[32m+[m[32m      src: '_gh_pages/**/*.html'[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    watch: {[m
[32m+[m[32m      src: {[m
[32m+[m[32m        files: '<%= jshint.core.src %>',[m
[32m+[m[32m        tasks: ['jshint:src', 'qunit', 'concat'][m
[32m+[m[32m      },[m
[32m+[m[32m      test: {[m
[32m+[m[32m        files: '<%= jshint.test.src %>',[m
[32m+[m[32m        tasks: ['jshint:test', 'qunit'][m
[32m+[m[32m      },[m
[32m+[m[32m      less: {[m
[32m+[m[32m        files: 'less/**/*.less',[m
[32m+[m[32m        tasks: 'less'[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    sed: {[m
[32m+[m[32m      versionNumber: {[m
[32m+[m[32m        pattern: (function () {[m
[32m+[m[32m          var old = grunt.option('oldver');[m
[32m+[m[32m          return old ? RegExp.quote(old) : old;[m
[32m+[m[32m        })(),[m
[32m+[m[32m        replacement: grunt.option('newver'),[m
[32m+[m[32m        recursive: true[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    'saucelabs-qunit': {[m
[32m+[m[32m      all: {[m
[32m+[m[32m        options: {[m
[32m+[m[32m          build: process.env.TRAVIS_JOB_ID,[m
[32m+[m[32m          throttled: 10,[m
[32m+[m[32m          maxRetries: 3,[m
[32m+[m[32m          maxPollRetries: 4,[m
[32m+[m[32m          urls: ['http://127.0.0.1:3000/js/tests/index.html?hidepassed'],[m
[32m+[m[32m          browsers: grunt.file.readYAML('grunt/sauce_browsers.yml')[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    exec: {[m
[32m+[m[32m      npmUpdate: {[m
[32m+[m[32m        command: 'npm update'[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m
[32m+[m[32m    compress: {[m
[32m+[m[32m      main: {[m
[32m+[m[32m        options: {[m
[32m+[m[32m          archive: 'bootstrap-<%= pkg.version %>-dist.zip',[m
[32m+[m[32m          mode: 'zip',[m
[32m+[m[32m          level: 9,[m
[32m+[m[32m          pretty: true[m
[32m+[m[32m        },[m
[32m+[m[32m        files: [[m
[32m+[m[32m          {[m
[32m+[m[32m            expand: true,[m
[32m+[m[32m            cwd: 'dist/',[m
[32m+[m[32m            src: ['**'],[m
[32m+[m[32m            dest: 'bootstrap-<%= pkg.version %>-dist'[m
[32m+[m[32m          }[m
[32m+[m[32m        ][m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m
[32m+[m[32m  // These plugins provide necessary tasks.[m
[32m+[m[32m  require('load-grunt-tasks')(grunt, { scope: 'devDependencies' });[m
[32m+[m[32m  require('time-grunt')(grunt);[m
[32m+[m
[32m+[m[32m  // Docs HTML validation task[m
[32m+[m[32m  grunt.registerTask('validate-html', ['jekyll:docs', 'htmllint']);[m
[32m+[m
[32m+[m[32m  var runSubset = function (subset) {[m
[32m+[m[32m    return !process.env.TWBS_TEST || process.env.TWBS_TEST === subset;[m
[32m+[m[32m  };[m
[32m+[m[32m  var isUndefOrNonZero = function (val) {[m
[32m+[m[32m    return val === undefined || val !== '0';[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Test task.[m
[32m+[m[32m  var testSubtasks = [];[m
[32m+[m[32m  // Skip core tests if running a different subset of the test suite[m
[32m+[m[32m  if (runSubset('core') &&[m
[32m+[m[32m      // Skip core tests if this is a Savage build[m
[32m+[m[32m      process.env.TRAVIS_REPO_SLUG !== 'twbs-savage/bootstrap') {[m
[32m+[m[32m    testSubtasks = testSubtasks.concat(['dist-css', 'dist-js', 'csslint:dist', 'test-js', 'docs']);[m
[32m+[m[32m  }[m
[32m+[m[32m  // Skip HTML validation if running a different subset of the test suite[m
[32m+[m[32m  if (runSubset('validate-html') &&[m
[32m+[m[32m      // Skip HTML5 validator on Travis when [skip validator] is in the commit message[m
[32m+[m[32m      isUndefOrNonZero(process.env.TWBS_DO_VALIDATOR)) {[m
[32m+[m[32m    testSubtasks.push('validate-html');[m
[32m+[m[32m  }[m
[32m+[m[32m  // Only run Sauce Labs tests if there's a Sauce access key[m
[32m+[m[32m  if (typeof process.env.SAUCE_ACCESS_KEY !== 'undefined' &&[m
[32m+[m[32m      // Skip Sauce if running a different subset of the test suite[m
[32m+[m[32m      runSubset('sauce-js-unit') &&[m
[32m+[m[32m      // Skip Sauce on Travis when [skip sauce] is in the commit message[m
[32m+[m[32m      isUndefOrNonZero(process.env.TWBS_DO_SAUCE)) {[m
[32m+[m[32m    testSubtasks.push('connect');[m
[32m+[m[32m    testSubtasks.push('saucelabs-qunit');[m
[32m+[m[32m  }[m
[32m+[m[32m  grunt.registerTask('test', testSubtasks);[m
[32m+[m[32m  grunt.registerTask('test-js', ['jshint:core', 'jshint:test', 'jshint:grunt', 'jscs:core', 'jscs:test', 'jscs:grunt', 'qunit']);[m
[32m+[m
[32m+[m[32m  // JS distribution task.[m
[32m+[m[32m  grunt.registerTask('dist-js', ['concat', 'uglify:core', 'commonjs']);[m
[32m+[m
[32m+[m[32m  // CSS distribution task.[m
[32m+[m[32m  grunt.registerTask('less-compile', ['less:compileCore', 'less:compileTheme']);[m
[32m+[m[32m  grunt.registerTask('dist-css', ['less-compile', 'autoprefixer:core', 'autoprefixer:theme', 'usebanner', 'csscomb:dist', 'cssmin:minifyCore', 'cssmin:minifyTheme']);[m
[32m+[m
[32m+[m[32m  // Full distribution task.[m
[32m+[m[32m  grunt.registerTask('dist', ['clean:dist', 'dist-css', 'copy:fonts', 'dist-js']);[m
[32m+[m
[32m+[m[32m  // Default task.[m
[32m+[m[32m  grunt.registerTask('default', ['clean:dist', 'copy:fonts', 'test']);[m
[32m+[m
[32m+[m[32m  // Version numbering task.[m
[32m+[m[32m  // grunt change-version-number --oldver=A.B.C --newver=X.Y.Z[m
[32m+[m[32m  // This can be overzealous, so its changes should always be manually reviewed![m
[32m+[m[32m  grunt.registerTask('change-version-number', 'sed');[m
[32m+[m
[32m+[m[32m  grunt.registerTask('build-glyphicons-data', function () { generateGlyphiconsData.call(this, grunt); });[m
[32m+[m
[32m+[m[32m  // task for building customizer[m
[32m+[m[32m  grunt.registerTask('build-customizer', ['build-customizer-html', 'build-raw-files']);[m
[32m+[m[32m  grunt.registerTask('build-customizer-html', 'jade');[m
[32m+[m[32m  grunt.registerTask('build-raw-files', 'Add scripts/less files to customizer.', function () {[m
[32m+[m[32m    var banner = grunt.template.process('<%= banner %>');[m
[32m+[m[32m    generateRawFiles(grunt, banner);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  grunt.registerTask('commonjs', 'Generate CommonJS entrypoint module in dist dir.', function () {[m
[32m+[m[32m    var srcFiles = grunt.config.get('concat.bootstrap.src');[m
[32m+[m[32m    var destFilepath = 'dist/js/npm.js';[m
[32m+[m[32m    generateCommonJSModule(grunt, srcFiles, destFilepath);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Docs task.[m
[32m+[m[32m  grunt.registerTask('docs-css', ['autoprefixer:docs', 'autoprefixer:examples', 'csscomb:docs', 'csscomb:examples', 'cssmin:docs']);[m
[32m+[m[32m  grunt.registerTask('lint-docs-css', ['csslint:docs', 'csslint:examples']);[m
[32m+[m[32m  grunt.registerTask('docs-js', ['uglify:docsJs', 'uglify:customize']);[m
[32m+[m[32m  grunt.registerTask('lint-docs-js', ['jshint:assets', 'jscs:assets']);[m
[32m+[m[32m  grunt.registerTask('docs', ['docs-css', 'lint-docs-css', 'docs-js', 'lint-docs-js', 'clean:docs', 'copy:docs', 'build-glyphicons-data', 'build-customizer']);[m
[32m+[m
[32m+[m[32m  grunt.registerTask('prep-release', ['jekyll:github', 'compress']);[m
[32m+[m
[32m+[m[32m  // Task for updating the cached npm packages used by the Travis build (which are controlled by test-infra/npm-shrinkwrap.json).[m
[32m+[m[32m  // This task should be run and the updated file should be committed whenever Bootstrap's dependencies change.[m
[32m+[m[32m  grunt.registerTask('update-shrinkwrap', ['exec:npmUpdate', '_update-shrinkwrap']);[m
[32m+[m[32m  grunt.registerTask('_update-shrinkwrap', function () {[m
[32m+[m[32m    var done = this.async();[m
[32m+[m[32m    npmShrinkwrap({ dev: true, dirname: __dirname }, function (err) {[m
[32m+[m[32m      if (err) {[m
[32m+[m[32m        grunt.fail.warn(err);[m
[32m+[m[32m      }[m
[32m+[m[32m      var dest = 'test-infra/npm-shrinkwrap.json';[m
[32m+[m[32m      fs.renameSync('npm-shrinkwrap.json', dest);[m
[32m+[m[32m      grunt.log.writeln('File ' + dest.cyan + ' updated.');[m
[32m+[m[32m      done();[m
[32m+[m[32m    });[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[1mdiff --git a/bower_components/bootstrap/LICENSE b/bower_components/bootstrap/LICENSE[m
