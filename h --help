[32m+[m[32mdefine([[m
[32m+[m	[32m"./core"[m
[32m+[m[32m], function( jQuery ) {[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Optional (non-Sizzle) selector module for custom builds.[m
[32m+[m[32m *[m
[32m+[m[32m * Note that this DOES NOT SUPPORT many documented jQuery[m
[32m+[m[32m * features in exchange for its smaller size:[m
[32m+[m[32m *[m
[32m+[m[32m * Attribute not equal selector[m
[32m+[m[32m * Positional selectors (:first; :eq(n); :odd; etc.)[m
[32m+[m[32m * Type selectors (:input; :checkbox; :button; etc.)[m
[32m+[m[32m * State-based selectors (:animated; :visible; :hidden; etc.)[m
[32m+[m[32m * :has(selector)[m
[32m+[m[32m * :not(complex selector)[m
[32m+[m[32m * custom selectors via Sizzle extensions[m
[32m+[m[32m * Leading combinators (e.g., $collection.find("> *"))[m
[32m+[m[32m * Reliable functionality on XML fragments[m
[32m+[m[32m * Requiring all parts of a selector to match elements under context[m
[32m+[m[32m *   (e.g., $div.find("div > *") now matches children of $div)[m
[32m+[m[32m * Matching against non-elements[m
[32m+[m[32m * Reliable sorting of disconnected nodes[m
[32m+[m[32m * querySelectorAll bug fixes (e.g., unreliable :focus on WebKit)[m
[32m+[m[32m *[m
[32m+[m[32m * If any of these are unacceptable tradeoffs, either use Sizzle or[m
[32m+[m[32m * customize this stub for the project's specific needs.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar docElem = window.document.documentElement,[m
[32m+[m	[32mselector_hasDuplicate,[m
[32m+[m	[32mmatches = docElem.matches ||[m
[32m+[m		[32mdocElem.webkitMatchesSelector ||[m
[32m+[m		[32mdocElem.mozMatchesSelector ||[m
[32m+[m		[32mdocElem.oMatchesSelector ||[m
[32m+[m		[32mdocElem.msMatchesSelector,[m
[32m+[m	[32mselector_sortOrder = function( a, b ) {[m
[32m+[m		[32m// Flag for duplicate removal[m
[32m+[m		[32mif ( a === b ) {[m
[32m+[m			[32mselector_hasDuplicate = true;[m
[32m+[m			[32mreturn 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar compare = b.compareDocumentPosition && a.compareDocumentPosition && a.compareDocumentPosition( b );[m
[32m+[m
[32m+[m		[32mif ( compare ) {[m
[32m+[m			[32m// Disconnected nodes[m
[32m+[m			[32mif ( compare & 1 ) {[m
[32m+[m
[32m+[m				[32m// Choose the first element that is related to our document[m
[32m+[m				[32mif ( a === document || jQuery.contains(document, a) ) {[m
[32m+[m					[32mreturn -1;[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( b === document || jQuery.contains(document, b) ) {[m
[32m+[m					[32mreturn 1;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Maintain original order[m
[32m+[m				[32mreturn 0;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn compare & 4 ? -1 : 1;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Not directly comparable, sort on existence of method[m
[32m+[m		[32mreturn a.compareDocumentPosition ? -1 : 1;[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32mjQuery.extend({[m
[32m+[m	[32mfind: function( selector, context, results, seed ) {[m
[32m+[m		[32mvar elem, nodeType,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mresults = results || [];[m
[32m+[m		[32mcontext = context || document;[m
[32m+[m
[32m+[m		[32m// Same basic safeguard as Sizzle[m
[32m+[m		[32mif ( !selector || typeof selector !== "string" ) {[m
[32m+[m			[32mreturn results;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Early return if context is not an element or document[m
[32m+[m		[32mif ( (nodeType = context.nodeType) !== 1 && nodeType !== 9 ) {[m
[32m+[m			[32mreturn [];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( seed ) {[m
[32m+[m			[32mwhile ( (elem = seed[i++]) ) {[m
[32m+[m				[32mif ( jQuery.find.matchesSelector(elem, selector) ) {[m
[32m+[m					[32mresults.push( elem );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m} else {[m
[32m+[m			[32mjQuery.merge( results, context.querySelectorAll(selector) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn results;[m
[32m+[m	[32m},[m
[32m+[m	[32munique: function( results ) {[m
[32m+[m		[32mvar elem,[m
[32m+[m			[32mduplicates = [],[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32mj = 0;[m
[32m+[m
[32m+[m		[32mselector_hasDuplicate = false;[m
[32m+[m		[32mresults.sort( selector_sortOrder );[m
[32m+[m
[32m+[m		[32mif ( selector_hasDuplicate ) {[m
[32m+[m			[32mwhile ( (elem = results[i++]) ) {[m
[32m+[m				[32mif ( elem === results[ i ] ) {[m
[32m+[m					[32mj = duplicates.push( i );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mwhile ( j-- ) {[m
[32m+[m				[32mresults.splice( duplicates[ j ], 1 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn results;[m
[32m+[m	[32m},s.
